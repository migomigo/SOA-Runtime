			<xs:complexType name="DiscountRule" abstract="true">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>Discount Rule</xs:documentation>
				</xs:annotation>
				<xs:sequence>


					<xs:element minOccurs="1" name="ruleId" type="xs:string">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								String. Required. Unique Id of the rule.
								This rule Id will be used to identify
								the rule used in ItemInfo. For MQ items,
								addlQtyFee can be defined through a
								rule. For half items, addl qty can be
								defined through rule.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="1" name="discountOption" type="xs:string">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								FLAT_ADDL_AMOUNT - Highest for first $x
								(=FLAT_ADDL_AMOUNT) for each
								additional.FLAT_ADDL_FREE = Highest for
								first and each additional is
								freeFLAT_ADDL_OFF_AMOUNT = Highest for
								first and $x (=FLAT_ADDL_OFF_AMOUNT) off
								for each additionalFLAT_PERCENT_OFF =
								Flat - Highest for first and %x
								(=FLAT_PERCENT_OFF) off for each
								additionalCALC_DO_NOT_COMBINE_WEIGHT =
								Use Individual Item
								weight.CACL_COMBINE_WEIGHT = Calc - Use
								Combined item
								weight.CALC_WEIGHT_OFF_VALUE = Calc -
								Highest for first and x oz
								(=CALC_WEIGHT_OFF_VALUE)off for each
								additionalFLAT and CALC rule id should
								be specified in TxnInfo. PROMO and
								HANDLING_RULE id is global (forced) and
								should not be specified in
								TxnInfo.PROMO_SHIPPING_COST_FOR_ITEM_TOTAL
								= Promo - Charge a shipping cost of $x
								(=PROMOTIONAL_SHIPPING_COST) for
								multiple purchases totaling over
								$y.PROMO_SHIPPING_COST_FOR_ITEM_COUNT =
								Promo - Charge a shipping cost of $x
								(=PROMOTIONAL_SHIPPING_COST) for
								purchases of y or more
								itemsPROMO_MAX_SHIPPING_COST = Promo -
								Do not charge more than $x
								(=PROMOTIONAL_SHIPPING_COST) for
								shipping for multiple purchases on a
								single order.HANDLING_RULE = Calculated
								Handling Type
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="DiscountRulesContainer">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
				<xs:sequence>


					<xs:element minOccurs="0" maxOccurs="unbounded" name="flatDiscountRules" type="tns:FlatAdditionalAmountRule">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								FLAT_ADDL_AMOUNT - Highest for first $x
								(=FLAT_ADDL_AMOUNT) for each additional.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" maxOccurs="unbounded" name="mqDiscountRules" type="tns:MQAdditionalAmountRule">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								List of MQAdditionalAmountRule.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" maxOccurs="unbounded" name="itemCountPromoRules" type="tns:ItemCountPromoRule">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								PROMO_SHIPPING_COST_FOR_ITEM_COUNT =
								Promo - Charge a shipping cost of $x
								(=PROMOTIONAL_SHIPPING_COST) for
								purchases of y or more items.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" maxOccurs="unbounded" name="givenTotalPromoRules" type="tns:GivenTotalPromoRule">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								PROMO_SHIPPING_COST_FOR_ITEM_TOTAL =
								Promo - Charge a shipping cost of $x
								(=PROMOTIONAL_SHIPPING_COST) for
								multiple purchases totaling over $y.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" maxOccurs="unbounded" name="packageHandlingRules" type="tns:PackageHandlingRule">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								List of PackageHandlingRule.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" maxOccurs="unbounded" name="itemAddlCostHandlingRules" type="tns:ItemAddlCostHandlingRule">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								List of ItemAddlCostHandlingRule.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="FlatAdditionalAmountRule">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>
						Flat Additional Amount Rule
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:DiscountRule">
						<xs:sequence>
							<xs:element name="addlAmount" type="xs:double">
								<xs:annotation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												CalculateShippingCost
											</CallName>
											<RequiredInput>
												Yes
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
									<xs:documentation>
										Required if discountOption is
										FLAT_ADDL_AMOUNT
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="GivenTotalPromoRule">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>
						Given Total Promo Rule
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:PromoRule">
						<xs:sequence>
							<xs:element name="subTotal" type="xs:double">
								<xs:annotation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												CalculateShippingCost
											</CallName>
											<RequiredInput>
												Yes
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
									<xs:documentation>
										Required if discountOption is
										PROMO_SHIPPING_COST_FOR_GIVEN_TOTAL.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="HandlingRule">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>Handling Rule</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:DiscountRule">
						<xs:sequence>
							<xs:element name="orderHandlingCost" type="xs:double">
								<xs:annotation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												CalculateShippingCost
											</CallName>
											<RequiredInput>
												Yes
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
									<xs:documentation>
										Required if discountOption is
										HANDLING_RULE.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="ItemAddlCostHandlingRule">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>
						Item Additional Cost Handling Rule
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:HandlingRule">
						<xs:sequence>
							<xs:element name="addlAmount" type="xs:double">
								<xs:annotation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												CalculateShippingCost
											</CallName>
											<RequiredInput>
												Yes
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
									<xs:documentation>
										Required if discountOption is
										CALC_ADDL_HANDLING_AMOUNT.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="ItemCountPromoRule">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>
						Item Count Promo Rule
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:PromoRule">
						<xs:sequence>
							<xs:element name="itemCount" type="xs:integer">
								<xs:annotation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												CalculateShippingCost
											</CallName>
											<RequiredInput>
												Yes
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
									<xs:documentation>
										tbd
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="MQAdditionalAmountRule">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:DiscountRule">
						<xs:sequence>
							<xs:element name="addlAmount" type="xs:double">
								<xs:annotation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												CalculateShippingCost
											</CallName>
											<RequiredInput>
												Yes
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
									<xs:documentation>
										Required if discountOption is
										FLAT_ADDL_AMOUNT
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="PackageHandlingRule">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>
						Package Handling Rule
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:HandlingRule">
						<xs:sequence>
							<xs:element name="packageHandlingCost" type="xs:double">
								<xs:annotation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												CalculateShippingCost
											</CallName>
											<RequiredInput>
												Yes
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
									<xs:documentation>
										Package Handling Cost
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="PromoRule">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>Promo Rule</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:DiscountRule">
						<xs:sequence>
							<xs:element name="promoShippingCost" type="xs:double">
								<xs:annotation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												CalculateShippingCost
											</CallName>
											<RequiredInput>
												Yes
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
									<xs:documentation>
										Required if discountOption is
										PROMO_SHIPPING_COST_FOR_GIVEN_TOTAL
										or
										PROMO_SHIPPING_COST_FOR_ITEMS_COUNT
										or PROMO_MAX_SHIPPING_COST.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:simpleType name="ShippingDeliveryType">
				<xs:annotation>
					<xs:documentation>
						Shipping Delivery Type, set to Residential all
						the time.
					</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Residential"/>
					<xs:enumeration value="Commercial"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:complexType name="ShippingOrder">
				<xs:annotation>
					<xs:documentation/>
					<xs:appinfo>
						<Summary>
							Occurrences one or more. These are list of
							orders for which shipping cost info is
							required. There will be no relationship
							between each order while processing. Each
							order will be processed indpendently.
							Support for muliple order is provided for
							optimum performance of client/server
							especially in case of calculated items.
						</Summary>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>


					<xs:element minOccurs="1" name="id" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								Unique id for every order.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="1" name="evaluationType" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								One of the following - COMMON_ONLY,
								DEFAULT_SELECTED, SELECTED, CHEAPEST,
								FASTEST. COMMON_ONLY applicable for RYP.
								DEFAULT_SELECTED applicable for NGXO
								first time call. SELECTED applicable for
								subsequent NGXO call. CHEAPEST, FASTESET
								applicable for new cart.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="txnInfo" type="tns:TransactionInfo">
						<xs:annotation>
							<xs:documentation>
								Required for calculated service
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="effectiveTime" type="xs:long">
						<xs:annotation>
							<xs:documentation>
								Optional. Unix time in GMT. Default -
								Current Unix time in GMT. Get rate for
								specific time.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="1" name="fromAddress" type="tns:Address">
						<xs:annotation>
							<xs:documentation>
								Country specified in fromAddress should
								match country for the siteId.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="1" name="toAddress" type="tns:Address">
						<xs:annotation>
							<xs:documentation>
								Buyer's destination address.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="discountRulesContainer" type="tns:DiscountRulesContainer">
						<xs:annotation>
							<xs:documentation>
								DiscountRulesContainer.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="sellerId" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								Optional. If passed, eBay can store the
								stats. Not used in any business logic.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="currencyId" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								Currency Id
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="insuranceOption" type="tns:InsuranceOption">
						<xs:annotation>
							<xs:documentation>
								Package Level Insurance Option,
								calculated only.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>
										CalculateShippingCost
									</CallName>
									<RequiredInput>No</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="ShippingServiceDetail">
				<xs:annotation>
					<xs:documentation/>
					<xs:appinfo>
						<Summary/>
					</xs:appinfo>
