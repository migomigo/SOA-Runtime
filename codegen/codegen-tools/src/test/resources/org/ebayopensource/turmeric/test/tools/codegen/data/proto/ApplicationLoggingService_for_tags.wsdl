			<xs:element name="logStatisticsRequest"
				type="tns:LogStatisticsRequest">

			</xs:element>
			<xs:complexType name="LogMessageRequest">
				<xs:annotation>
					<xs:documentation>
						Request for a message to be logged. Contains the
						name of the request, log level,
						information about the client that is making
						this log request (clientInfo), id used for
						correlating this log message request to other log messages
						(parentCorrelationId), actual data to be logged
						(data), complete message information to be
						logged(message), start timestamp (starttime),
						duration for this log.
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
						<xs:sequence>
							<xs:element name="name" type="xs:string">
								<xs:annotation>
									<xs:documentation>
										The name of this particular transaction.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="level" type="tns:LogLevel">
								<xs:annotation>
									<xs:documentation>
										The log level associated
										with this log message.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="clientInfo"
								type="tns:ClientInfo" >
								<xs:annotation>
									<xs:documentation>
										Information about the client which
										is making this log request.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="parentCorrelationId"
								type="xs:string" >
								<xs:annotation>
									<xs:documentation>
										The correlation id of the
										transaction inside which this
										log message request is raised.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="data" type="xs:string">
								<xs:annotation>
									<xs:documentation>
										Actual data to be logged for the request.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="message"
								type="tns:MessageType"  minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>
										The message to be
										logged.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="startTime"
								type="xs:dateTime" >
								<xs:annotation>
									<xs:documentation>
										Start time of the log message.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="duration" type="xs:float">
								<xs:annotation>
									<xs:documentation>
										The time taken to complete
										the event or the transaction.
									</xs:documentation>
								</xs:annotation>
							</xs:element>

						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="LogMessageResponse">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse" />
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="BeginTransactionRequest">
				<xs:annotation>
					<xs:documentation>
						Request for beginning a transaction. Contains
						the type of transaction, name of the transaction, information about
						the client which is starting this transaction
						request (clientInfo), id used for correlating
						this transaction request with respect to other
						transactions (parentCorrelationId), actual data
						to be logged (data).
						Note: This operation has not been implemented.
					</xs:documentation>
					<xs:appinfo>
						<summary>
							Request for beginning a transaction.This operation has not been implemented.
						</summary>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
						<xs:sequence>
							<xs:element name="type" type="xs:token">
								<xs:annotation>
									<xs:documentation>
										The type of application
										that has been accessed through
										this transaction.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="name" type="xs:string">
								<xs:annotation>
									<xs:documentation>
										The name of this particular transaction.									
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="clientInfo"
								type="tns:ClientInfo" >
								<xs:annotation>
									<xs:documentation>
										Information about the client which is starting the transaction. The information includes
										client details and client
										location.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="parentCorrelationId"
								type="xs:string" >
								<xs:annotation>
									<xs:documentation>
										The correlation id of the
										transaction to which this
										transaction is a child.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="data" type="xs:string">
								<xs:annotation>
									<xs:documentation>
										Data of the transaction.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="BeginTransactionResponse">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse">
						<xs:sequence>
							<xs:element name="correlationId"
								type="xs:string" >
								<xs:annotation>
									<xs:documentation>
										An identifier
										for the transaction that was
										begun - this id can be later
										used to access this transaction.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="EndTransactionRequest">
				<xs:annotation>
					<xs:documentation>
						Request for ending a transaction. The request contains an id used for
						identifying the transaction (correlationId),
						actual data to be logged (data), duration for
						this transaction, the status of this transaction.
						Note: This operation has not been implemented.
					</xs:documentation>
					<xs:appinfo>
						<summary>
							Request for ending a transaction.This operation has not been implemented.
						</summary>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
						<xs:sequence>
							<xs:element name="correlationId"
								type="xs:string" >
								<xs:annotation>
									<xs:documentation>
										The correlation id of the
										transaction which needs to be
										ended.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="data" type="xs:string">
								<xs:annotation>
									<xs:documentation>
										Data of the transaction.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="duration" type="xs:float">
								<xs:annotation>
									<xs:documentation>
										Time taken to complete
										the event or the transaction.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="status" type="xs:string">
								<xs:annotation>
									<xs:documentation>
										Case-sensitive character string
										which represents the status,
										first non-"0" is used.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="EndTransactionResponse">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse" />
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="LogStatisticsRequest">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
						<xs:sequence>
							<xs:element name="clientId" type="xs:string">
								<xs:annotation>
									<xs:documentation>
										Id which categorizes the client,like
										Mobile, appserver etc
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="statisticsDetail"
								type="tns:StatisticsDetail" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>
										Details about the statistics, such as its name and related data.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="StatisticsDetail">
				<xs:sequence>
					<xs:element name="name" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								Name of the Statistics,
								eg. CPUMetrics.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="data" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								Data (statistics) that needs to be
								logged.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="LogStatisticsResponse">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse" />
				</xs:complexContent>
			</xs:complexType>
			<xs:simpleType name="EnvironmentType">
				<xs:annotation>
					<xs:documentation>
						Client environment.
					</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="DEV">
						<xs:annotation>
							<xs:documentation>
								Development environment
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="TEST">
						<xs:annotation>
							<xs:documentation>
								QA environment
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="STAGING">
						<xs:annotation>
							<xs:documentation>
								Pre-release environemnt
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="SANDBOX">
						<xs:annotation>
							<xs:documentation>
								Sandbox environment
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="PRODUCTION">
						<xs:annotation>
							<xs:documentation>
								Production environment
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType name="AckValue">
				<xs:annotation>
					<xs:documentation>
						Indicates whether the error is a severe error
						(causing the request to fail) or an
						informational error (a warning) that should be
						communicated to the user.
					</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Success">
						<xs:annotation>
							<xs:documentation>
								The request that triggered the error was
								processed successfully.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Failure">
						<xs:annotation>
							<xs:documentation>
								The request that triggered the error was
								not processed successfully. When a
								serious application-level error occurs,
								the error is returned instead of the
								business data.
						    </xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Warning">
						<xs:annotation>
							<xs:documentation>
								The request that triggered the error was
								processed successfully but with some
								warnings.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="PartialFailure">
						<xs:annotation>
							<xs:documentation>
								The request was processed with partial success;
								some partial failure occurred that may affect
								your application or the user.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
			<xs:complexType abstract="true" name="BaseServiceRequest">
				<xs:annotation>
					<xs:documentation>
						This is the base class for request container for
						all service operations.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>

						processContents="lax" />
				</xs:sequence>
			</xs:complexType>
			<xs:complexType abstract="true"
				name="BaseServiceResponse">
				<xs:annotation>
					<xs:documentation>
						Base response container for all service
						operations. Contains error information
						associated with the request.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="ack" type="tns:AckValue">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<AllCalls />
									<Returned>Always</Returned>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								Acknowledgement for the logging- whether
								the logging was a Success or a Failure.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="errorMessage"
						type="tns:ErrorMessage" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<callInfo>
									<AllCalls />
									<Returned>Conditionally</Returned>
								</callInfo>
							</xs:appinfo>
							<xs:documentation>
								Information for an error or warning that
								occurred when eBay processed the
								request.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="version" type="xs:string"
						minOccurs="1">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<AllCalls />
									<Returned>Always</Returned>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								The service version used
								for logging.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="timestamp" type="xs:dateTime">
						<xs:annotation>
							<xs:appinfo>
								<CallInfo>
									<AllCalls />
									<Returned>Always</Returned>
								</CallInfo>
							</xs:appinfo>
							<xs:documentation>
								The time when the response
								is returned from the service.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

						processContents="lax" />
				</xs:sequence>
			</xs:complexType>
			<xs:simpleType name="ErrorCategory">
				<xs:annotation>
					<xs:documentation>
						Indicates the error category of an error.
						Typical values are "System","Application", and
						"Request"
					</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="System" />
					<xs:enumeration value="Application" />
					<xs:enumeration value="Request" />
				</xs:restriction>
			</xs:simpleType>
			<xs:complexType name="ErrorData">
				<xs:annotation>
					<xs:appinfo />
					<xs:documentation>
						Details about the error represented as error message contents.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="errorId" type="xs:long">
						<xs:annotation>
							<xs:documentation>
								Represents the ID which is associated
								with an error message.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="domain" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								Denotes the domain to which this error
								message is to be associated.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="subdomain" type="xs:string"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Denotes the sub-domain to which this
								error message is to be associated.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="severity"
						type="tns:ErrorSeverity">
						<xs:annotation>
							<xs:documentation>
								Denotes the severity of this error.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="category"
						type="tns:ErrorCategory">
						<xs:annotation>
							<xs:documentation>
								Denotes the category to which this error
								belongs.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="message" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								the error message which needs to be
								returned.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="exceptionId" type="xs:token">
						<xs:annotation>
							<xs:documentation>
								In case of exceptions, this denotes the
								exception id and the same will be used
								for tracking this error.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="parameter"
						type="tns:ErrorParameter" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
								Information on the set of data
								that caused the error/exception. Represented as a list of string values that is appended to the error message.
							</xs:documentation>
							<xs:appinfo>
								<callInfo>
									<AllCalls />
									<Returned>Conditionally</Returned>
								</callInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="ErrorMessage">
				<xs:annotation>
					<xs:appinfo />
					<xs:documentation>
						Represents the error message obtained during
						logging.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="error" type="tns:ErrorData"
						minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
								Details about the error.
							</xs:documentation>
							<xs:appinfo>
								<callInfo>
									<AllCalls />
									<Returned>Conditionally</Returned>
								</callInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>	
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="ErrorParameter">
				<xs:annotation>
					<xs:appinfo />
					<xs:documentation>
						The payload information or data 
						that provides information on the set of data
						that caused the error/exception.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleContent>
					<xs:extension base="xs:string">
						<xs:attribute name="name" type="xs:string"
							use="optional">
							<xs:annotation>
								<xs:documentation>
									The name attribute of the error parameter.
								</xs:documentation>
								<xs:appinfo>
									<callInfo>
										<AllCalls />
										<Returned>Always</Returned>
									</callInfo>
								</xs:appinfo>
							</xs:annotation>
						</xs:attribute>		
					</xs:extension>
				</xs:simpleContent>
			</xs:complexType>
			<xs:simpleType name="ErrorSeverity">
				<xs:annotation>
					<xs:documentation>
						Represents the severity of an error message
						obtained during logging.
					</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Error">
						<xs:annotation>
							<xs:documentation>
								Error severity is Error.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>	
					<xs:enumeration value="Warning">
						<xs:annotation>
							<xs:documentation>
								Error severity is Warning.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
			<xs:complexType name="MessageType">
				<xs:annotation>
					<xs:documentation>
						Contains the complete information
						about the message which is being logged.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="parentId" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								The id of the parent thread
								of this log message thread.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="id" type="xs:string" >
						<xs:annotation>
							<xs:documentation>
								The id of the thread which is logging a
								message.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="level" type="tns:LogLevel">
						<xs:annotation>
							<xs:documentation>
								Log level associated with this
								log message.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="name" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								Name used by by a client
								for logging the message.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="duration" type="xs:float">
						<xs:annotation>
							<xs:documentation>
								The time taken to complete the
								event or transaction of logging the message.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="startTime" type="xs:dateTime">
						<xs:annotation>
							<xs:documentation>
								Start time of the log operation for this
								message.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="data" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								The log message to be logged.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="AppDetails">
				<xs:annotation>
					<xs:documentation>
