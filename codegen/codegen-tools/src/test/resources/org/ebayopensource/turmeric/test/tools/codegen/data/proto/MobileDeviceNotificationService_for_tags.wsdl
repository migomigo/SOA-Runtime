	<wsdl:portType name="MobileDeviceNotificationServicePort">

		<wsdl:operation name="setDeviceNotificationPreferences">
			<wsdl:documentation>
				For the given user, set the device notification
				preferences. There is only one set of notification
				preferences for a user across all devices. The service
				will record the binding of the presented device token
				with the given user and will send notifications received
				for that user to the device. A user may be bound to more
				than one device token. All such devices will receive the
				same notifications
			</wsdl:documentation>
			<wsdl:input
				message="tns:setDeviceNotificationPreferencesRequest">
			</wsdl:input>
			<wsdl:output
				message="tns:setDeviceNotificationPreferencesResponse">
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="getDeviceNotificationPreferences">
			<wsdl:documentation>
				Get the notification preferences for user on a
				particular device. The result indicates whether or not
				notifications for this user are active on the device,
				and independent of that, what the device notification
				preferences are for this user.
			</wsdl:documentation>
			<wsdl:input
				message="tns:getDeviceNotificationPreferencesRequest">
			</wsdl:input>
			<wsdl:output
				message="tns:getDeviceNotificationPreferencesResponse">
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="activateUserOnDevice">
			<wsdl:documentation>
				Activate notifications for a user on a device using
				existing settings. By default, when the preferences are
				set, they are active. However, the device should call
				deactivateUserOnDevice when a user logs out, the
				application closes or the user disables notifications.
				Callinig activateUserOnDevice is how notifications are
				turned back on
			</wsdl:documentation>
			<wsdl:input message="tns:activateUserOnDeviceRequest"></wsdl:input>
			<wsdl:output message="tns:activateUserOnDeviceResponse"></wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="deactivateUserOnDevice">
			<wsdl:documentation>
				Deactive sending notifications for the given user to the
				specified device
			</wsdl:documentation>
			<wsdl:input message="tns:deactivateUserOnDeviceRequest"></wsdl:input>
			<wsdl:output message="tns:deactivateUserOnDeviceResponse"></wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="suspendDeviceNotificationsForUser">
			<wsdl:documentation>
				Suspend device notifications for the specified user for
				all devices
			</wsdl:documentation>
			<wsdl:input
				message="tns:suspendDeviceNotificationsForUserRequest">
			</wsdl:input>
			<wsdl:output
				message="tns:suspendDeviceNotificationsForUserResponse">
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="suspendNotificationsForDevice">
			<wsdl:documentation>
				Stop sending notifications to the specified device for
				any users that have previously registered on this
				device.
			</wsdl:documentation>
			<wsdl:input
				message="tns:suspendNotificationsForDeviceRequest">
			</wsdl:input>
			<wsdl:output
				message="tns:suspendNotificationsForDeviceResponse">
			</wsdl:output>
		</wsdl:operation>

		<wsdl:operation name="listDevicesForUser">
			<wsdl:documentation>
				Return a list of user to device binding information
				showing what is known about devices to which the user
				has been bound
			</wsdl:documentation>
			<wsdl:input message="tns:listDevicesForUserRequest"></wsdl:input>
			<wsdl:output message="tns:listDevicesForUserResponse"></wsdl:output>
		</wsdl:operation>
	</wsdl:portType>






