			<xs:simpleType name="EventMessageLanguageTag">
				<xs:annotation>
					<xs:documentation>
						Langage to use for localizing the event message
						text. The tag should follow IETF BCP 47 as
						described at
						http://www.w3.org/International/articles/language-tags/.
						Examples: en-US, fr-CA, or de.
					</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:string"></xs:restriction>
			</xs:simpleType>

			<xs:element name="setDeviceNotificationPreferences">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="tns:BaseRequest">
							<xs:sequence>

								<xs:element name="userid"
									type="xs:string">
									<xs:annotation>
										<xs:documentation>
											The userid of the user for
											whom device notification
											preferences are to be set.
										</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="deviceHandle"
									type="tns:DeviceHandleType">
									<xs:annotation>
										<xs:documentation>
											the token and type of token
											information identifying the
											mobile device.
										</xs:documentation>
									</xs:annotation>
								</xs:element>

								<xs:element name="description"
									type="xs:string" maxOccurs="1" minOccurs="0">
									<xs:annotation>
										<xs:documentation>
											A human readable identification of the device that 
											generated the token.  A phone number or the user-assigned 
											device name might be useful here.  
										</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="language"
									type="tns:EventMessageLanguageTag">
									<xs:annotation>
										<xs:documentation>
											A tag describing the
											language that should be used
											to localize the event
											messages sent back to this
											device. The tag should
											follow IETF BCP 47 as
											described at
											http://www.w3.org/International/articles/language-tags/
											Examples include en-US,
											fr-CA and de.
										</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="preferences"
									type="tns:NotificationPreferencesType">
									<xs:annotation>
										<xs:documentation>
											The preferences to be set
											for this user. Note the user
											may only have one set of
											preferences across all
											subscribed devices.
											Notifications sent to all
											devices for this user will
											be governed by the
											preferences set here.
										</xs:documentation>
									</xs:annotation>
								</xs:element>

							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element
				name="setDeviceNotificationPreferencesResponse">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="tns:BaseResponse" />
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="getDeviceNotificationPreferences">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="tns:BaseRequest">
							<xs:sequence>
								<xs:element name="userid"
									type="xs:string">
									<xs:annotation>
										<xs:documentation>
											The userid for whom device
											notification preferences
											should be retrieved.
										</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="deviceHandle"
									type="tns:DeviceHandleType">
									<xs:annotation>
										<xs:documentation>
											This token identifies the
											device the user receives
											preferences on.
										</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element
				name="getDeviceNotificationPreferencesResponse">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="tns:BaseResponse">
							<xs:sequence>

								<xs:element name="active"
									type="xs:boolean">
									<xs:annotation>
										<xs:documentation>
											Indicates whether or not the
											user is receiving
											notifications on this
											particular device. Since the
											user/device binding must be
											established, it is possible
											that the user has some
											device notification
											preferences set, but is not
											actively receiving
											notifications on this
											particular device.
										</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="description"
									type="xs:string" maxOccurs="1" minOccurs="0">
									<xs:annotation>
										<xs:documentation>
											A human readable identification of the device that 
											generated the token.  A phone number or the user-assigned 
											device name might be useful here.  
										</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="language"
									type="tns:EventMessageLanguageTag">
									<xs:annotation>
										<xs:documentation>
											A tag describing the
											language used to localize
											the event messages sent back
											to this device. The tag
											should follow IETF BCP 47 as
											described at
											http://www.w3.org/International/articles/language-tags/
											Examples include en-US,
											fr-CA and de.
										</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="preferences"
									type="tns:NotificationPreferencesType" minOccurs="0"
									maxOccurs="1">
									<xs:annotation>
										<xs:documentation>
											The device notification
											preferences set for a user.
											If none have been set, then
											none is returned.
										</xs:documentation>
									</xs:annotation>
								</xs:element>

							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="activateUserOnDevice">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="tns:BaseRequest">
							<xs:sequence>

								<xs:element name="userid"
									type="xs:string">
									<xs:annotation>
										<xs:documentation>
											The userid of the user who
											is to start receiving
											notifications on this
											device. The preference must
											have previously been set.
										</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="deviceHandle"
									type="tns:DeviceHandleType">
									<xs:annotation>
										<xs:documentation>
											The device token identifying
											the device where the
											notifications should be
											sent.
										</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="description"
