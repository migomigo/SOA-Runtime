<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions 
	name            = "AdjustmentService"
    xmlns:wsdl      = "http://schemas.xmlsoap.org/wsdl/"
    xmlns           = "http://schemas.xmlsoap.org/wsdl/"
    xmlns:http      = "http://schemas.xmlsoap.org/wsdl/http/"
    xmlns:mime      = "http://schemas.xmlsoap.org/wsdl/mime/"
    xmlns:soap      = "http://schemas.xmlsoap.org/wsdl/soap/"
    xmlns:soap12    = "http://schemas.xmlsoap.org/wsdl/soap12/"
    xmlns:tns       = "http://www.ebay.com/marketplace/billing/v1/services"
    xmlns:xs        = "http://www.w3.org/2001/XMLSchema" 
    targetNamespace = "http://www.ebay.com/marketplace/billing/v1/services">
    
    <wsdl:types>
        <xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
            targetNamespace="http://www.ebay.com/marketplace/billing/v1/services">

		 <!-- Usage Adjustment Resquest -->
	     <xs:element     name="applyUsageAdjustmentRequest" type="tns:ApplyUsageAdjustmentRequest"/>
	     <xs:complexType name="ApplyUsageAdjustmentRequest">
	      <xs:annotation>
	         <xs:documentation>
	               Applies credit to a pre-existing usage transaction in the billing system.
	         </xs:documentation>
	      </xs:annotation>
	      <xs:complexContent>
	        <xs:extension base="tns:BaseBatchRequestType">
	          <xs:sequence>
	             <xs:element name="usageAdjustment" type="tns:UsageAdjustmentType" minOccurs="1" maxOccurs="unbounded"/>
	          </xs:sequence>
	        </xs:extension>
	       </xs:complexContent>
	     </xs:complexType>
	
	     <xs:complexType name="UsageAdjustmentType">
	       <xs:annotation>
	          <xs:documentation>
	              The element that represents, one single fee credit transaction that is sent to the billing system.
	            </xs:documentation>
	       </xs:annotation>
	       <xs:complexContent>
	         <xs:extension base="tns:BaseAdjustmentRequestType">
	         <xs:sequence>
		         <xs:element name="chargeId" type="xs:string" minOccurs="1">
		           <xs:annotation>
		              <xs:documentation>
		                     Usage Adjustment Identifier. This 
		                     uniquely identifies a charge
		                     transaction in the client's
		                     section.
		              </xs:documentation>
		           </xs:annotation>
		         </xs:element>
		         <xs:element name="chargeContextId" type="xs:string" minOccurs="0">
		           <xs:annotation>
		               <xs:documentation>
		                    Context of the Credit
		                    Identifier. This uniquely
		                    identifies where the credit was
		                    generated, and in what context.
		                    This can be any kind of grouping
		                    id, with which the credit is
		                    associated.
		               </xs:documentation>
		           </xs:annotation>
		         </xs:element>
	        </xs:sequence>
	       </xs:extension>
	      </xs:complexContent>
	     </xs:complexType>      
	     
	     <!-- Usage Adjustment Response -->
	     <xs:element     name="applyUsageAdjustmentResponse" type="tns:ApplyUsageAdjustmentResponse"/>
	     <xs:complexType name="ApplyUsageAdjustmentResponse">
	       <xs:annotation>
	       <xs:documentation>
	              Response for the credit that was applied against a usage transaction.
	       </xs:documentation>
	       </xs:annotation>
	         <xs:complexContent>
	            <xs:extension base="tns:BaseBatchResponseType">
	              <xs:sequence>
	                <xs:element name="usageAdjustmentResponse" type="tns:UsageAdjustmentResponseType" minOccurs="1" maxOccurs="unbounded"/>
	              </xs:sequence>
	            </xs:extension>
	       </xs:complexContent>
	     </xs:complexType>
	     
	     <xs:complexType name="UsageAdjustmentResponseType">                           
	       <xs:complexContent>
	         <xs:extension base="tns:BaseResponseType">
	           <xs:sequence>
	             <xs:element name="referenceId" type="xs:string" minOccurs="0">
	               <xs:annotation>
	                 <xs:documentation>
	                    Reference identifier. This is an
	                    optional element, that will be
	                    returned by the service, per
	                    tenant/client basis. If the
	                    credit identifier is generated
	                    by the service, and not the
	                    client, this field will be
	                    populated optionally.
	                 </xs:documentation>
	               </xs:annotation>
	             </xs:element>
	                          
	             <xs:element name="usageAdjustmentResponseDetails" type="tns:UsageAdjustmentResponseDetailsType" minOccurs="0"/>
	                           
	           </xs:sequence>
	         </xs:extension>
	       </xs:complexContent>
	     </xs:complexType>
	     
	     <!--  TODO: Do we even have this data coming back? No amount should be returned as double!  -->
	     <xs:complexType name="UsageAdjustmentResponseDetailsType">
	       <xs:annotation>
	           <xs:documentation>
	               The element that represents the credit response details. This is optional to begin with, which bubbles up repsonses.
	           </xs:documentation>
	       </xs:annotation>
	       <xs:sequence>
	         <xs:element name="creditRequested" type="xs:double" minOccurs="1">
	           <xs:annotation>
	             <xs:documentation>
	                 This is the Credit Amount Requested.
	             </xs:documentation>
	           </xs:annotation>
	         </xs:element>
	         <xs:element name="actualCreditIssued" type="xs:double" minOccurs="1">
	            <xs:annotation>
	              <xs:documentation>
	                 This is the Credit Amount Issued. 
	                 It is possible to be less than 
	                 creditRequested.
	              </xs:documentation>
	            </xs:annotation>
	         </xs:element>
	         <xs:element name="originalCharge" type="xs:double" minOccurs="1">
	            <xs:annotation>
	              <xs:documentation>
	                 This is the original charge 
	                 against which credit is being 
	                 requested.
	              </xs:documentation>
	            </xs:annotation>
	         </xs:element>
	         <xs:element name="totalPreviousCredits" type="xs:double" minOccurs="1">
	            <xs:annotation>
	            <xs:documentation>
	               This is the cumulative of all 
	               credits issued exccluding the 
	               latest credit requested.
	             </xs:documentation>
	             </xs:annotation>
	         </xs:element>
	         <xs:element name="totalCreditableTransactionBalance" type="xs:double" minOccurs="1">
	           <xs:annotation>
	             <xs:documentation>
	                This is the amount left after 
	                the all credits are deducted 
	                from original charges.
	             </xs:documentation>
	           </xs:annotation>
	         </xs:element>             		
	       </xs:sequence>
	     </xs:complexType>
	     
		<!-- Recurring Charge Adjustment Request -->
		
	     <xs:element     name="applyRecurringChargeAdjustmentRequest" type="tns:ApplyRecurringChargeAdjustmentRequest"/>
	     <xs:complexType name="ApplyRecurringChargeAdjustmentRequest">
	      <xs:annotation>
	         <xs:documentation>
	               Applies adjustment to an existing recurring charge transaction.
	         </xs:documentation>
	      </xs:annotation>
	      <xs:complexContent>
	        <xs:extension base="tns:BaseBatchRequestType">
	          <xs:sequence>
	             <xs:element name="recurringChargeAdjustment" type="tns:RecurringChargeAdjustmentType" minOccurs="1" maxOccurs="unbounded"/>
	          </xs:sequence>
	        </xs:extension>
	       </xs:complexContent>
	     </xs:complexType>
	
	     <xs:complexType name="RecurringChargeAdjustmentType">
	       <xs:annotation>
	          <xs:documentation>
	              The element that represents a single recurring charge adjustment that is sent to the billing system.
	          </xs:documentation>
	       </xs:annotation>
	       <xs:complexContent>
	         <xs:extension base="tns:BaseAdjustmentRequestType">
	         <xs:sequence>
	         	<xs:element name="recurringChargeId" type="tns:BilledTransactionIdType" minOccurs="1" >
	         		<xs:annotation>
	         			<xs:documentation>
	         				The transaction identifier for the transaction
	         				to which the recurring charge adjustment applies.
	         			</xs:documentation>
	         		</xs:annotation>
	         	</xs:element>
	         	<xs:element name="elementId" type="xs:long" minOccurs="1" >
	         	<xs:annotation>
	         		<xs:documentation>
	         			Product element identifier
	         		</xs:documentation>
	         	</xs:annotation>
	         </xs:element>      
	         
	         </xs:sequence>
	       </xs:extension>
	      </xs:complexContent>
	     </xs:complexType>    
	     
	     <!-- Non Recurring Charge Adjustment Request -->
	
	     <xs:element     name="applyNonRecurringChargeAdjustmentRequest" type="tns:ApplyNonRecurringChargeAdjustmentRequest" />
	     <xs:complexType name="ApplyNonRecurringChargeAdjustmentRequest">
	     	<xs:annotation>
	     		<xs:documentation>
	     			Applies adjustment to a non-recurring charge.
	     		</xs:documentation>
	     	</xs:annotation>
	     	<xs:complexContent>
	     		<xs:extension base="tns:BaseBatchRequestType">
	     			<xs:sequence>
	     				<xs:element name="nonRecurringChargeAdjustment" type="tns:NonRecurringChargeAdjustmentType" maxOccurs="unbounded" minOccurs="1"/>
	     			</xs:sequence>
	     		</xs:extension>
	     	</xs:complexContent>
	     </xs:complexType>
	
	     <xs:complexType name="NonRecurringChargeAdjustmentType">
	     	<xs:annotation>
	     		<xs:documentation>
	     			The element that represents a single adjustment that is
	     			sent to the billing system.
	     		</xs:documentation>
	     	</xs:annotation>
	     	<xs:complexContent>
	     		<xs:extension base="tns:BaseAdjustmentRequestType">
	     			<xs:sequence>
	    				<xs:element name="nonRecurrinChargeId" type="tns:BilledTransactionIdType" minOccurs="1">
	     					<xs:annotation>
	     						<xs:documentation>
	     							The transaction identifier for the transaction
	     							to which the non recurring charge adjustment applies.
	     						</xs:documentation>
	     					</xs:annotation>
	     				</xs:element>
	     				<xs:element name="adjustmentType" type="tns:AdjustmentTransactionTypeCode" minOccurs="1">
	     					<xs:annotation>
	     						<xs:documentation>
	     							Transaction code used to identify the type of
	     							adjustment to be applied to a non recurring charge. 
	     						</xs:documentation>
	     					</xs:annotation>
	     				</xs:element>
	
	     			</xs:sequence>
	     		</xs:extension>
	     	</xs:complexContent>
	     </xs:complexType>
	
	     <!-- Account Adjustment Request -->
	
	     <xs:element     name="applyAccountAdjustmentRequest" type="tns:ApplyAccountAdjustmentRequest" />
	     <xs:complexType name="ApplyAccountAdjustmentRequest">
	     	<xs:annotation>
	     		<xs:documentation>
	     			Applies adjustment to an account in the billing system.
	     		</xs:documentation>
	     	</xs:annotation>
	     	<xs:complexContent>
	     		<xs:extension base="tns:BaseBatchRequestType">
	     			<xs:sequence>
	     				<xs:element name="accountAdjustment" type="tns:AccountAdjustmentType" maxOccurs="unbounded" minOccurs="1"/>
	     			</xs:sequence>
	     		</xs:extension>
	     	</xs:complexContent>
	     </xs:complexType>
	
	     <xs:complexType name="AccountAdjustmentType">
	     	<xs:annotation>
	     		<xs:documentation>
	     			The element that represents a single account adjustment.
	     		</xs:documentation>
	     	</xs:annotation>
	     	<xs:complexContent>
	     		<xs:extension base="tns:BaseAdjustmentRequestType">
	     			<xs:sequence>
	     				<xs:element name="adjustmentType" type="tns:AdjustmentTransactionTypeCode" minOccurs="1">
	     					<xs:annotation>
	     						<xs:documentation>
	     							Transaction code used to identify the type of
	     							adjustment to be applied to an account. 
	     						</xs:documentation>
	     					</xs:annotation>
	     				</xs:element>
	     				<xs:element name="creditMode" type="tns:CreditMode" minOccurs="1" >
	     					<xs:annotation>
	     						<xs:documentation>
	     							Indicates whether or not the available credit amount is issued if the requested 
	     							amount is larger than the available credit balance. 
	     							CreditExactAmount: the requested amount is credited only it is less than or 
	     							equal to the available credit balance. Otherwise, the request amount is not credited.
	     							CreditAvailableAmount: if the requested amount is larger than the available credit balance,
	     							then the available credit amount is credited.
	     						</xs:documentation>
	     					</xs:annotation>
	     				</xs:element>
	     			</xs:sequence>
	     		</xs:extension>
	     	</xs:complexContent>
	     </xs:complexType>
	
	     <xs:simpleType name="AdjustmentTransactionTypeCode">
	     	<xs:restriction base="xs:long">
	     	    <xs:annotation>
	     		<xs:documentation>Transaction code used to identify the type of adjustment to be applied. </xs:documentation>
	     	    </xs:annotation>
	     	</xs:restriction>
	     </xs:simpleType>
	     
	     <xs:simpleType name="CreditMode">
	     	<xs:restriction base="xs:token">
	     	    <xs:annotation>
	     		<xs:documentation>
					Indicates the 2 credit modes: CreditExactAmount, CreditAvailableAmount.
	     		</xs:documentation>
	     	    </xs:annotation>
	     	</xs:restriction>
	     </xs:simpleType>
	     
	     <xs:complexType name="BilledTransactionIdType">
	     	<xs:annotation>
	     		<xs:documentation>
	     			This type identifies a transaction in an invoice. All
	     			the transactions in an invoice are billed transactions.
	     		</xs:documentation>
	     	</xs:annotation>
	     	<xs:sequence>
	     		<xs:element name="invoiceReference" type="tns:InvoiceReferenceType" minOccurs="1">
	     			<xs:annotation>
	     				<xs:documentation>
	     					Reference to the invoice.
	     				</xs:documentation>
	     			</xs:annotation>
	     		</xs:element>
	     		<xs:element name="invoiceRow" type="xs:long" minOccurs="1">
	     			<xs:annotation>
	     				<xs:documentation>
	     					Row identifier used to identify a row in an
	     					invoice.
	     				</xs:documentation>
	     			</xs:annotation>
	     		</xs:element>
	     	</xs:sequence>
	     </xs:complexType>
	
	     <xs:complexType name="InvoiceIdType">
	         <xs:annotation>
	             <xs:documentation>Identifies an invoice.</xs:documentation>
	         </xs:annotation>
	         <xs:sequence>
	             <xs:element name="InvoiceNumberMajor" type="xs:long" minOccurs="1">
	                 <xs:annotation>
	                     <xs:documentation>
		                     The second part of the composite primary key identifying 
		                     the specific invoice (BILL_INVOICE.bill_ref_no); the first part of the 
		                     composite primary key is the account number.
	                     </xs:documentation>
	                 </xs:annotation>
	             </xs:element>
	             <xs:element name="InvoiceNumberMinor" type="xs:int" minOccurs="1">
	                 <xs:annotation>
	                     <xs:documentation>
		                    The third part of the composite primary key identifying 
		                    the specific invoice (BILL_INVOICE.bill_ref_resets).
	                     </xs:documentation>
	                 </xs:annotation>
	             </xs:element>
	         </xs:sequence>
	     </xs:complexType>
	
	     <xs:complexType name="InvoiceReferenceType">
	         <xs:annotation>
	             <xs:documentation>Identifies an invoice.</xs:documentation>
	         </xs:annotation>
	         <xs:sequence>
	             <xs:element name="InvoiceId" type="tns:InvoiceIdType" minOccurs="1">
	             </xs:element>
	             <xs:element name="invoiceStatementDate" type="xs:dateTime" minOccurs="1">
	                 <xs:annotation>
	                     <xs:documentation>Invoice statement date.</xs:documentation>
	                 </xs:annotation>
	             </xs:element>
	         </xs:sequence>
	     </xs:complexType>
	
	     <xs:simpleType name="RequestStatus">
	     	<xs:restriction base="xs:token">
	     	    <xs:annotation>
	     		<xs:documentation>
					APPROVED: indicates approved adjustment request.
	     		</xs:documentation>
	     	    </xs:annotation>
	     	</xs:restriction>
	     </xs:simpleType>
	     
	     <!-- Adjustment Response -->
	     <xs:element     name="applyAdjustmentResponse" type="tns:ApplyAdjustmentResponse"/>
	     <xs:complexType name="ApplyAdjustmentResponse">
	       <xs:annotation>
	       <xs:documentation>
	              Response for the adjustment that was applied against a transaction or invoice.
	       </xs:documentation>
	       </xs:annotation>
	         <xs:complexContent>
	            <xs:extension base="tns:BaseBatchResponseType">
	              <xs:sequence>
	                <xs:element name="adjustmentResponse" type="tns:AdjustmentResponseType" minOccurs="1" maxOccurs="unbounded"/>
	              </xs:sequence>
	            </xs:extension>
	       </xs:complexContent>
	     </xs:complexType>     
	     
	     <xs:complexType name="AdjustmentResponseType">                           
	       <xs:complexContent>
	         <xs:extension base="tns:BaseResponseType">
	           <xs:sequence>
	             <xs:element name="referenceId" type="xs:string" minOccurs="0">
	               <xs:annotation>
	                 <xs:documentation>
	                    Reference identifier. This is an
	                    optional element, that will be
	                    returned by the service, per
	                    tenant/client basis. If the
	                    adjustment identifier is generated
	                    by the service, and not the
	                    client, this field will be
	                    populated optionally.
	                 </xs:documentation>
	               </xs:annotation>
	             </xs:element>
	                          
	             <xs:element name="adjustmentResponseDetails" type="tns:AdjustmentResponseDetailsType" minOccurs="0"/>
	                           
	           </xs:sequence>
	         </xs:extension>
	       </xs:complexContent>
	     </xs:complexType>
	     
	     <xs:complexType name="AdjustmentResponseDetailsType">
	     	<xs:annotation>
	     		<xs:documentation>
	     			The element that represents the adjustment response
	     			details. 
	     		</xs:documentation>
	     	</xs:annotation>
	     	<xs:sequence>
	     		<xs:element name="actualAdjustmentApplied" type="tns:Money" minOccurs="1">
	     			<xs:annotation>
	     				<xs:documentation>
	     					This is the actual adjustment amount applied. It is
	     					possible to be less than the amount requested.
	     				</xs:documentation>
	     			</xs:annotation>
	     		</xs:element>
	     	</xs:sequence>
	     </xs:complexType>
	     
	     <!--  TYPES DEFINED IN BILLING DOMAIN TYPE LIBRARY -->
	
	     <xs:complexType name="AccountIdType">
	       <xs:annotation>
	        <xs:documentation></xs:documentation>
	          <xs:appinfo>
	            <typeLibrarySource library="BillingTypeLibrary"
	               namespace="http://www.ebay.com/marketplace/billing/v1/services">
	            </typeLibrarySource>
	          </xs:appinfo>
	       </xs:annotation>
	       <xs:sequence>
	         <xs:element name="clientId"        type="xs:string" minOccurs="1">  </xs:element>
	         <xs:element name="accountId"       type="xs:string" minOccurs="1">  </xs:element>
	         <xs:element name="accountSuffixId" type="xs:string" minOccurs="1">  </xs:element>
	       </xs:sequence>
	     </xs:complexType>
	     
	        <xs:complexType name="RequestHeaderType">
	          <xs:annotation>
	            <xs:documentation></xs:documentation>
	            <xs:appinfo>
	               <typeLibrarySource library="BillingTypeLibrary"
	                  namespace="http://www.ebay.com/marketplace/billing/v1/services">
	               </typeLibrarySource>
	            </xs:appinfo>
	          </xs:annotation>
	          <xs:sequence>
	            <xs:element name="requestId"     type="xs:string">  </xs:element>
	            <xs:element name="requestTime"   type="xs:dateTime"></xs:element>
	          </xs:sequence>
	        </xs:complexType>
	
	        <xs:complexType name="ResponseHeaderType">
	          <xs:annotation>
	            <xs:documentation></xs:documentation>
	            <xs:appinfo>
	              <typeLibrarySource library="BillingTypeLibrary"
	                 namespace="http://www.ebay.com/marketplace/billing/v1/services">
	              </typeLibrarySource>
	            </xs:appinfo>
	          </xs:annotation>
	          <xs:sequence>
	            <xs:element name="requestId"      type="xs:string">                 </xs:element>
	          </xs:sequence>
	        </xs:complexType>
	
	        <xs:complexType name="BaseRequestType">
	          <xs:complexContent>
	            <xs:extension base="tns:BaseServiceRequest">
	            <xs:annotation>
	              <xs:documentation></xs:documentation>
	                <xs:appinfo>
	                  <typeLibrarySource library="BillingTypeLibrary"
	                   namespace="http://www.ebay.com/marketplace/billing/v1/services">
	                  </typeLibrarySource>
	                </xs:appinfo>
	              </xs:annotation>
	              <xs:sequence>
	                <xs:element name="requestHeader" type="tns:RequestHeaderType"> </xs:element>
	                <xs:element name="accountId"     type="tns:AccountIdType">     </xs:element>
	              </xs:sequence>
	            </xs:extension>
	          </xs:complexContent>
	        </xs:complexType>
	
	        <xs:complexType name="BaseAdjustmentRequestType"> 
	           	<xs:complexContent>
	        		<xs:extension base="tns:BaseRequestType">
	        			<xs:annotation>
	        				<xs:documentation>
	        					The common request type that represents the adjustment details
	        				</xs:documentation>
	        				<xs:appinfo>
	        					<typeLibrarySource
	        						library="BillingTypeLibrary"
	        						namespace="http://www.ebay.com/marketplace/billing/v1/services">
	        					</typeLibrarySource>
	        				</xs:appinfo>
	        			</xs:annotation>
	        			<xs:sequence>
	
	        				<xs:element name="adjustmentAmount" type="tns:Money" minOccurs="1">
	        					<xs:annotation>
	        						<xs:documentation>
	        							Amount of the adjustment requested.
	        						</xs:documentation>
	        					</xs:annotation>
	        				</xs:element>
	
	        				<xs:element name="reasonCode" type="xs:int" minOccurs="1">
	        					<xs:annotation>
	        						<xs:documentation>
	        							Pre-defined reason code,
	        							establishing the reason for the
	        							adjustment.
	        						</xs:documentation>
	        					</xs:annotation>
	        				</xs:element>
	
	        				<xs:element name="adjustmentDate" type="xs:dateTime" minOccurs="1">
	        					<xs:annotation>
	        						<xs:documentation>
	        							Date on which the adjustment was
	        							generated on the client system.
	        						</xs:documentation>
	        					</xs:annotation>
	        				</xs:element>
	
	        				<xs:element name="adjustmentRequestStatus" type="tns:RequestStatus" minOccurs="1">
	        					<xs:annotation>
	        						<xs:documentation>
	        							Status of an adjustment request;
	        						</xs:documentation>
	        					</xs:annotation>
	        				</xs:element>
	
	        				<xs:element name="submitter" type="xs:string" minOccurs="0">
	        					<xs:annotation>
	        						<xs:documentation>
	        							Id or name of the person who is submitting
	        							the adjustment request.
	        						</xs:documentation>
	        					</xs:annotation>
	        				</xs:element>
	
	        				<xs:element name="memo" type="xs:string" minOccurs="0">
	        					<xs:annotation>
	        						<xs:documentation>
	        							Any description associated with the adjustment.
	        						</xs:documentation>
	        					</xs:annotation>
	        				</xs:element>
	
	        			</xs:sequence>
	        		</xs:extension>
	        	</xs:complexContent>
	        </xs:complexType>
			
	        <xs:complexType name="BaseResponseType">
	          <xs:complexContent>
	            <xs:extension base="tns:BaseServiceResponse">
	              <xs:annotation>
	                <xs:documentation></xs:documentation>
	                <xs:appinfo>
	                  <typeLibrarySource library="BillingTypeLibrary"
	                    namespace="http://www.ebay.com/marketplace/billing/v1/services">
	                  </typeLibrarySource>
	                </xs:appinfo>
	              </xs:annotation>
	              <xs:sequence>
	                <xs:element name="responseHeader" type="tns:ResponseHeaderType"> </xs:element>
	              </xs:sequence>
	           </xs:extension>
	          </xs:complexContent>
	        </xs:complexType>
	        
	        <xs:complexType name="BaseBatchRequestType">
	          <xs:annotation>
	            <xs:documentation></xs:documentation>
	              <xs:appinfo>
	                <typeLibrarySource library="BillingTypeLibrary"
	                     namespace="http://www.ebay.com/marketplace/billing/v1/services">
	                </typeLibrarySource>
	              </xs:appinfo>
	          </xs:annotation>
	           <xs:sequence>
	              <xs:element name="batchId"             type="xs:string"  minOccurs="0">    </xs:element>                
	           </xs:sequence>
	        </xs:complexType>
	        
	        <xs:complexType name="BaseBatchResponseType">
	          <xs:annotation>
	          <xs:documentation></xs:documentation>
