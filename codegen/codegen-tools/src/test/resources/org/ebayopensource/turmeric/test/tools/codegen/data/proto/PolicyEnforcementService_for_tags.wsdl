	<wsdl:service name="PolicyEnforcementService">
		<xs:annotation>
			<xs:appinfo>
				<ns0:version>1.0</ns0:version>
			</xs:appinfo>
			<wsdl:documentation>
				This service provides the ability to enforce security
				policies.
				<Version>1.0.0</Version>
			</wsdl:documentation>
		</xs:annotation>
