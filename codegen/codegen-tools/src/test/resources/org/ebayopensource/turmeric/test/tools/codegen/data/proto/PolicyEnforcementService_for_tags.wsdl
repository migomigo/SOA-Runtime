				-->
				<xs:sequence>
					<xs:element name="error" type="tns:ErrorData" minOccurs="0" maxOccurs="unbounded">
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			
			<xs:complexType name="ErrorParameter">
				<xs:annotation>
					<xs:documentation>
