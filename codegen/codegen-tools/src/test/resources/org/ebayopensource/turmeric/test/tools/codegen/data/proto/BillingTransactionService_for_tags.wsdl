       <xs:complexType name="UsageChargeType">
         <xs:annotation>
            <xs:documentation>
              The element that represents, one single fee transaction that is sent to the billing system.
            </xs:documentation>
         </xs:annotation>
         <xs:complexContent>
           <xs:extension base="tns:BaseRequestType">
           <xs:sequence>

             <xs:element name="chargeId" type="xs:string">
               <xs:annotation>
                 <xs:documentation>
                     Charge Identifier. This
                     uniquely identifies a charge
                     transaction in the client's
                     section.
                 </xs:documentation>
               </xs:annotation>
             </xs:element>

             <xs:element name="chargeContextId" type="xs:string">
               <xs:annotation>
                 <xs:documentation>
                    Context of the Charge
                    Identifier. This uniquely
                    identifies where the charge was
                    generated, and in what context.
                    This can be any kind of grouping
                    id, with which the charge is
                    associated.
                 </xs:documentation>
               </xs:annotation>
             </xs:element>

             <xs:element name="chargeType" type="xs:int" minOccurs="1">
               <xs:annotation>
                  <xs:documentation>
                        Identifier for a type of
                       charge. This will be the action
                       code defined by the Billing
                       system, and published per
                       client.
                  </xs:documentation>
               </xs:annotation>
             </xs:element>

             <xs:element name="chargeProviderId" type="xs:int" minOccurs="1">
               <xs:annotation>
                 <xs:documentation>
                    Site where the charge
                    originated. Also referred as
                    Provider identifier, and is
                    client specific value, pre
                    configured in the billing
                    system.
                 </xs:documentation>
               </xs:annotation>
             </xs:element>

             <xs:element name="chargeCategory" type="xs:int" minOccurs="1">
               <xs:annotation>
                 <xs:documentation>
                     Category of the charge.
                    This is pre-configured for client
                    in the billing system.
                 </xs:documentation>
               </xs:annotation>
             </xs:element>

             <xs:element name="chargeDate" type="xs:dateTime">
               <xs:annotation>
                 <xs:documentation>
                     Date when the charge was
                    created.
                 </xs:documentation>
               </xs:annotation>
             </xs:element>

             <xs:element name="chargeAmount" type="tns:Money">
               <xs:annotation>
                 <xs:documentation>
                      Amount and the currency
                     associated with it the charge.
                 </xs:documentation>
               </xs:annotation>
             </xs:element>

             <xs:element name="memo" type="xs:string" minOccurs="0">
               <xs:annotation>
                 <xs:documentation>
                      Any additional text
                     description associated with the
                     charge. But not the title.
                 </xs:documentation>
               </xs:annotation>
             </xs:element>

           </xs:sequence>
         </xs:extension>
       </xs:complexContent>
     </xs:complexType>


