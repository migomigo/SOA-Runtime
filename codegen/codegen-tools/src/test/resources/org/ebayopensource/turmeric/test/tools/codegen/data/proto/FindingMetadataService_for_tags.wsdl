<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="FindingMetadataService"
	targetNamespace="http://www.ebay.com/marketplace/search/v1/services"
	xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.ebay.com/marketplace/search/v1/services"
	xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns="http://schemas.xmlsoap.org/wsdl/">

	<wsdl:types>

		<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
			attributeFormDefault="unqualified" elementFormDefault="qualified"
			targetNamespace="http://www.ebay.com/marketplace/search/v1/services"
			xmlns:tns="http://www.ebay.com/marketplace/search/v1/services">

			<xs:element name="getVersionRequest" type="tns:GetVersionRequest" />
			<xs:element name="getVersionResponse" type="tns:GetVersionResponse" />
			<xs:element name="getProductTypesByNamesRequest" type="tns:GetProductTypesByNamesRequest" />
			<xs:element name="getProductTypesByNamesResponse" type="tns:GetProductTypesByNamesResponse" />
			<xs:element name="getCategoryPropertiesRequest" type="tns:GetCategoryPropertiesRequest" />
			<xs:element name="getCategoryPropertiesResponse" type="tns:GetCategoryPropertiesResponse" />
			<xs:element name="getCatalogPropertiesRequest" type="tns:GetCatalogPropertiesRequest" />
			<xs:element name="getCatalogPropertiesResponse" type="tns:GetCatalogPropertiesResponse" />
			<xs:element name="getGlobalDomainRequest" type="tns:GetGlobalDomainRequest" />
			<xs:element name="getGlobalDomainResponse" type="tns:GetGlobalDomainResponse" />

			<xs:complexType name="GetVersionRequest">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:complexType name="GetVersionResponse">
				<xs:annotation>
					<xs:documentation>
						Document goes here
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="1" name="output"
								type="xs:string" />
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="GetProductTypesByNamesRequest">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="1" name="context"
								type="tns:ServiceContext">
								<xs:annotation>
								   <xs:documentation>
									   Contextual information used by Search services. The information
									   is regarding the environment in which a service is running,
									   such as site, cobrand, locale, timezone, and currency.
								   </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="1" name="name"
								type="xs:string">
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="1" name="loadProperties"
								type="xs:boolean">
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="GetProductTypesByNamesResponse">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="0" name="executionTime"
								type="xs:long">
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="1"
								name="productType" type="tns:ProductType">
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="0" name="property"
								type="tns:Property">
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:complexType name="GetCategoryPropertiesRequest">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="1" name="context"
								type="tns:ServiceContext">
								<xs:annotation>
								   <xs:documentation>
									   Contextual information used by Search services. The information
									   is regarding the environment in which a service is running,
									   such as site, cobrand, locale, timezone, and currency.
								   </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="1" name="category"
								type="tns:CategoryKey">
								<xs:annotation>
									<xs:documentation>
										The category of a product, represented by a unique category key (id).
									</xs:documentation>
								</xs:annotation>
								</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:complexType name="GetCategoryPropertiesResponse">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="0" name="executionTime"
								type="xs:long">
								<xs:annotation>
									<xs:documentation>
										Time (in milliseconds) taken to execute the request.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="categoryProperty" type="tns:CategoryProperty">
								<xs:annotation>
									<xs:documentation>
										Property related to the specified category.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:complexType name="GetCatalogPropertiesRequest">
				<xs:annotation>
					<xs:documentation>
						Retrieves catalog properties for the specified
						catalodId(s), usecase and dataset(s).
						If dataset(s) is/are not specifed, then all
						datasets associated with the usecase and the catalogId
						applicable for the site and the partner are returned.
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
						<xs:sequence>
							<xs:element name="context" type="tns:ServiceContext"
								maxOccurs="1" minOccurs="1">
								<xs:annotation>
									<xs:documentation>
										Contextual information used by Search services. The information
										is regarding the environment in which a service is running,
										such as site, cobrand, locale, timezone, and currency.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="1" name="catalogId"
								type="xs:long">
								<xs:annotation>
									<xs:documentation>
										Catalog id.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="usecase" type="xs:string" maxOccurs="1"
								minOccurs="1">
								<xs:annotation>
									<xs:documentation>
										Name of usecase	(as defined in UsecaseEnum).
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="dataset" type="xs:string" maxOccurs="unbounded"
								minOccurs="0">
								<xs:annotation>
									<xs:documentation>
										Name of the dataset	that the property belongs to(as defined in DatasetEnum).
										If it is not specified, then the properties for all the data sets are returned.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:complexType name="GetCatalogPropertiesResponse">
				<xs:annotation>
					<xs:documentation>
						Response containing the list of catalog properties.
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="0" name="executionDuration"
								type="xs:long">
								<xs:annotation>
									<xs:documentation>
										Time taken to execute GetCatalogPropertiesRequest.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="catalogPropertyList" type="tns:CatalogProperties">
								<xs:annotation>
									<xs:documentation>
										Catalog properties for the specified
										catalodId(s), usecase and dataset(s).
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:complexType name="GetGlobalDomainRequest">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="1" name="context"
								type="tns:ServiceContext">
								<xs:annotation>
								   <xs:documentation>
									   Contextual information used by Search services. The information
									   is regarding the environment in which a service is running,
									   such as site, cobrand, locale, timezone, and currency.
								   </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="1" name="loadProperties"
								type="xs:boolean">
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:complexType name="GetGlobalDomainResponse">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="0" name="executionTime"
								type="xs:long">
								<xs:annotation>
									<xs:documentation>Time (in milliseconds)taken to execute the request.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="1" name="globalDomain"
								type="tns:GlobalDomain">
								<xs:annotation>
									<xs:documentation>
										A collection of all the eBay site offices.
								    </xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>



			<xs:complexType abstract="true" name="Domain">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary">
						</typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="1" name="name"
						type="xs:string">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="displayName"
						type="xs:string">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="seoName"
						type="xs:string">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="id" type="xs:long">
					</xs:element>
				</xs:sequence>
			</xs:complexType>





			<xs:complexType name="CategoryKey">
				<xs:annotation>
					<xs:documentation>The category of a product, represented by a unique category key (id). </xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary">
						</typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:Key">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="1" name="categoryId"
								type="xs:string">
								<xs:annotation>
									<xs:documentation>Unique identifier for the category.</xs:documentation>
								</xs:annotation>		
							</xs:element>
						</xs:sequence>

					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:complexType name="CategoryProperty">
				<xs:annotation>
					<xs:documentation>Container for the property related to the category.</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary">
						</typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="1" name="category"
						type="tns:CategoryKey">
						<xs:annotation>
							<xs:documentation>Unique category key as specified in the request.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="property"
						type="tns:Property">
						<xs:annotation>
							<xs:documentation>Property of the category.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="AspectValue">
				<xs:annotation>
					<xs:documentation>
						
					</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary">
						</typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="1" name="name"
						type="xs:string">
						<xs:annotation>
							<xs:documentation>
								A value name for a given aspect. 
								For example, "Point and Shoot" is a value name for the "Product Type" aspect in the "Digital Cameras" domain.
							</xs:documentation>
						</xs:annotation>	
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="displayName"
						type="xs:string">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="rank"
						type="xs:int">
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="property"
						type="tns:Property">
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="GlobalDomain">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary">
						</typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:Domain">
						<xs:sequence>
							<xs:element maxOccurs="unbounded" minOccurs="0" name="aspect"
								type="tns:GlobalAspect">
								<xs:annotation>
									<xs:documentation>
										A characteristic of an item in the global domain.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="0" name="property"
								type="tns:Property">
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>


			<xs:complexType name="AspectDependency">
				<xs:annotation>
					<xs:documentation>
						This is the type for AspectDependency as defined
						in Magellan domain metadata.
					</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary">
						</typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="0"
						name="parentValue" type="xs:string">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="parentAspectName"
						type="xs:string">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="childAspectName"
						type="xs:string">
					</xs:element>

				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="DomainAspect">
				<xs:annotation>
					<xs:documentation>
						This is the type for Aspect as defined in
						Magellan domain metadata.
					</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary">
						</typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="1" name="name"
						type="xs:string">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="namespace"
						type="xs:token">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="displayName"
						type="xs:string">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="pluralDisplayName"
						type="xs:string">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="rank"
						type="xs:int">
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0"
						name="aspectValue" type="tns:AspectValue">
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="property"
						type="tns:Property">
					</xs:element>

					<xs:element maxOccurs="unbounded" minOccurs="0"
						name="childDependency" type="tns:AspectDependency">
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0"
						name="parentDependency" type="tns:AspectDependency">
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="ProductType">
				<xs:annotation>
					<xs:documentation>
						This is the type for the Product Type in Magellan
						Domain Metadata.
					</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:Domain">
						<xs:sequence>
							<xs:element maxOccurs="unbounded" minOccurs="0" name="aspect"
								type="tns:DomainAspect"></xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="0" name="property"
								type="tns:Property"></xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="GlobalAspect">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:DomainAspect">
						<xs:sequence>
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="exclusiveGroup" type="xs:string">
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="inclusiveGroup" type="xs:string">
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="parentInclusiveGroup" type="xs:string">
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="siblingOrGroup" type="xs:string">
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="killExclusiveGroup" type="xs:string">
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="ServiceContext">
				<xs:annotation>
					<xs:documentation>
						Contextual information used by Search Services.
			</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceContext">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="0" name="locale"
								type="xs:token">
								<xs:annotation>
									<xs:documentation>
										Locale in the format of &quot;language,country,variant&quot;. Check
										java.util.Locale
					</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="0" name="timeZone"
								type="xs:token">
								<xs:annotation>
									<xs:documentation>
										Time zone in the format of &quot;GMT+/-hh:mm&quot;
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="0" name="currency"
								type="xs:token">
								<xs:annotation>
									<xs:documentation>
										Money currency. Check com.ebay.af.common.types.Currency for the
										available values.		
					</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="Property">
				<xs:annotation>
					<xs:documentation>
						Name value pair for defining properties associated with entities.
					</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchServicesTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/typelibrary"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="1" name="name"
						type="xs:string">
						<xs:annotation>
							<xs:documentation>
								The name of the property.
					</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="value"
						type="xs:string">
						<xs:annotation>
							<xs:documentation>
								The values of the property.
					</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:simpleType name="AckValue">
				<xs:annotation>
					<xs:documentation>
						Indicates whether the error is a severe error (causing the request to fail)
						or an informational error (a warning) that should be communicated
						to the
						user.
			</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="MarketPlaceServiceCommonTypeLibrary"
							namespace="http://www.ebay.com/marketplace/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:restriction base="xs:string">

					<xs:enumeration value="Success">
						<xs:annotation>
							<xs:documentation>
								The request was processed successfully.				
					</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Failure">
						<xs:annotation>

							<xs:documentation>
								The request that triggered the error was not processed
								successfully.
								When a serious application-level error occurs, the error is returned
								instead of the business data.
					</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Warning">
						<xs:annotation>
							<xs:documentation>
								The request that triggered the error was processed successfully but
								with some warnings.
					</xs:documentation>

						</xs:annotation>
