			<xs:complexType name="DomainFilter">
				<xs:annotation>
					<xs:documentation>
						Name of the domain to which search results will be restricted.
					</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="SearchTypeLibrary"
							namespace="http://www.ebay.com/marketplace/search/v1/services">
