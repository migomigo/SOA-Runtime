			<wsdl:input>
				<mime:content part="createCheckoutCart" type="text/xml" />
			</wsdl:input>
			<wsdl:output>
				<mime:content part="createCheckoutCart" type="text/xml" />
			</wsdl:output>
		</wsdl:operation>
