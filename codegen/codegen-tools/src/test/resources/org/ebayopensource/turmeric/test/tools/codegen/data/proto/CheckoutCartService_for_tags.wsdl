			<http:operation
				location="CheckoutCartService/purchaseCheckoutCart" />
			<wsdl:input>
				<mime:content part="purchaseCheckoutCart"
					type="text/xml" />
			</wsdl:input>
			<wsdl:output>
				<mime:content part="purchaseCheckoutCart"
					type="text/xml" />
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="getCheckoutCart">
			<http:operation
				location="CheckoutCartService/getCheckoutCart" />
			<wsdl:input>
				<mime:content part="getCheckoutCart" type="text/xml" />
			</wsdl:input>
			<wsdl:output>
				<mime:content part="getCheckoutCart" type="text/xml" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:binding name="CheckoutCartServiceSoapBinding"
		type="tns:CheckoutCartServicePort">
		<soap12:binding style="document"
