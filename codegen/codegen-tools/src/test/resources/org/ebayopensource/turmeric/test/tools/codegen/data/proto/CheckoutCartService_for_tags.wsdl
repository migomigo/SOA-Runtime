			<xs:complexType name="CreateCheckoutCartRequest">
				<xs:annotation>
					<xs:documentation>
						Creates a checkout cart containing the specified
						items.
					</xs:documentation>
					<xs:appinfo>
						<Summary></Summary>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
						<xs:sequence>
							<xs:element name="cartId" type="xs:long">
								<xs:annotation>
									<xs:documentation>
										The ID of the cart to be
										created. Should be Unique for
										each cart.
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												createCheckoutCart
											</CallName>
											<RequiredInput>
												Yes
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="1"
								maxOccurs="unbounded" name="lineItem" type="tns:LineItem">
								<xs:annotation>
									<xs:documentation>
										The Items to be purchased.
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												createCheckoutCart
											</CallName>
											<RequiredInput>
												Yes
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0"
								name="flowStartStep" type="xs:token">
								<xs:annotation>
									<xs:documentation>
										Where in the checkout flow to
										start with this cart.
									</xs:documentation>
									<xs:appinfo>
										<ListBasedOn>
											CheckoutFlowStartStep
										</ListBasedOn>
										<Default>
											ReviewYourPurchase
										</Default>
										<CallInfo>
											<CallName>
												createCheckoutCart
											</CallName>
											<RequiredInput>
												No
											</RequiredInput>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>


			<xs:complexType name="GetCheckoutCartRequest">
