	<wsdl:portType name="CheckoutCartServicePort">
		<wsdl:operation name="createCheckoutCart">
			<wsdl:documentation>
				Create a checkout cart
			</wsdl:documentation>
			<wsdl:input message="tns:createCheckoutCartRequest" />
			<wsdl:output message="tns:createCheckoutCartResponse" />
		</wsdl:operation>
		<wsdl:operation name="purchaseCheckoutCart">
			<wsdl:documentation>
				Purchase the checkout cart
			</wsdl:documentation>
			<wsdl:input message="tns:purchaseCheckoutCartRequest" />
			<wsdl:output message="tns:purchaseCheckoutCartResponse" />
		</wsdl:operation>
		<wsdl:operation name="getCheckoutCart">
			<wsdl:documentation>
				Retrieve a checkout cart by cart id
			</wsdl:documentation>
			<wsdl:input message="tns:getCheckoutCartRequest" />
			<wsdl:output message="tns:getCheckoutCartResponse" />
		</wsdl:operation>
	</wsdl:portType>

