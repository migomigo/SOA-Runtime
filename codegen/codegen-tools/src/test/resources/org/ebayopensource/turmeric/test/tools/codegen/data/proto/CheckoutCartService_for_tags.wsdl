		<wsdl:part name="params"
			element="tns:purchaseCheckoutCartRequest" />
	</wsdl:message>
	<wsdl:message name="purchaseCheckoutCartResponse">
		<wsdl:part name="params"
			element="tns:purchaseCheckoutCartResponse" />
	</wsdl:message>
