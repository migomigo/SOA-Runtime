	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	name="DeviceConfigurationService"
	targetNamespace="http://www.ebay.com/marketplace/mobile/v1/services">
	<wsdl:documentation>
		Service to get the configuration settings for an application.
	</wsdl:documentation>
	<wsdl:types>
		<xs:schema attributeFormDefault="unqualified"
			elementFormDefault="qualified"
			targetNamespace="http://www.ebay.com/marketplace/mobile/v1/services">
			<xs:element name="getVersionResponse"
				type="tns:GetVersionResponse" />
			<xs:complexType name="GetVersionResponse">
				<xs:annotation>
					<xs:documentation>
						The version of this service
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseResponse">
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:complexType name="GetConfigurationRequest">
				<xs:annotation>
					<xs:documentation>
						The request to get an application's
						configuration
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseRequest">
						<xs:sequence>
							<xs:element name="applicationName"
								type="xs:string" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>
										The name of the mobile device.
										Examples are iPhone, Nokia e63,
										BlackBerry Curve 8900.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="applicationVersion"
								type="xs:string" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>
										The software version of the
										application running on the
										mobile device.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element
								name="currentConfigurationVersion" type="xs:string"
								minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>
										represents the configuration
										version that the requesting
										client currently holds.

										If this version is different
										from the latest version
										maintained by the service, a
										response with the latest set of
										properties is sent back to the
										client.

										No validation is performed upon
										this field. If an application
										wishes to get the latest
										configuration properties, it is
										recommended that this field be
										set to the empty string.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="siteId" type="xs:int"
								minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>
										siteId represents an eBay Site
										ID. It is an integer field and
										expected to match with an eBay
										Site. The default value is 0
										which is the eBay US site.

										This field is validated. If the
										provided value does not match
										with an existing eBay Site, the
										response will be an error.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:complexType name="GetConfigurationResponse">
				<xs:annotation>
					<xs:documentation>
						Includes the latest configuration version and
						list of properties. The configurationVersion is
						always equal to the latest configuration
						version.

						If the properties list is empty, it means that
						either (a) the configurationVersion in this
						response and the configurationVersion in the
						request are the same and that the client already
						has the latest set of configuration properties
						or (b) this configurationVersion defines no
						properties.

						In cases where an invalid configurationVersion
						is given in the request, it is assumed that the
						client does not have the latest configuration so
						the latest response is returned.

						This response has an AckValue of FAILURE if and
						only if the other request parameters are
						erroneous or invalid.
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseResponse">
						<xs:sequence>
							<xs:element name="configurationVersion"
								type="xs:string" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>
										The latest configurationVersion
										in use
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="configuration"
								type="tns:Configuration" minOccurs="0" maxOccurs="unbounded">
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:element name="getConfigurationRequest"
				type="tns:GetConfigurationRequest">

				<xs:annotation>
					<xs:documentation>
						Represents the request to get the latest
						configuration of an application
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="getConfigurationResponse"
				type="tns:GetConfigurationResponse">

			</xs:element>

			<xs:complexType name="Property">
				<xs:annotation>
					<xs:documentation>
						A property is a simple pair of name and value.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="name" type="xs:string"
						minOccurs="1" maxOccurs="1">
					</xs:element>
					<xs:element name="value" type="xs:string"
						minOccurs="1" maxOccurs="1">
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="GetVersionRequest">
				<xs:complexContent>
					<xs:extension base="tns:BaseRequest"></xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:element name="getVersionRequest"
				type="tns:GetVersionRequest">
			</xs:element>

