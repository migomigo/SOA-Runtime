			<xs:complexType name="Configuration">
				<xs:sequence>
					<xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1"></xs:element>
					<xs:element name="property" type="tns:Property" minOccurs="0" maxOccurs="unbounded"></xs:element>
