			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="getVersion">

			<soap:operation
				soapAction="http://www.ebay.com/marketplace/mobile/v1/services/getVersion" />
			<wsdl:input>

				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>

				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="getConfiguration">

			<soap:operation
				soapAction="http://www.ebay.com/marketplace/mobile/v1/services/getConfiguration" />
			<wsdl:input>

				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>

				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="DeviceConfigurationService">
		<wsdl:documentation>
			<Version>1.0.0</Version>
			The DeviceConfigurationService provides a service for
			application developers to retrieve configuration settings
			for a mobile application.
		</wsdl:documentation>
		<wsdl:port binding="tns:DeviceConfigurationServiceHttpBinding"
			name="DeviceConfigurationServiceHttpPort">
			<http:address
				location="https://svcs.ebay.com/services/mobile/v1/DeviceConfigurationService" />
		</wsdl:port>
		<wsdl:port binding="tns:DeviceConfigurationServiceSOAPBinding"
			name="DeviceConfigurationServiceSOAPPort">
			<soap12:address
				location="https://svcs.ebay.com/services/mobile/v1/DeviceConfigurationService" />
			<soap:address
				location="https://svcs.ebay.com/services/mobile/v1/DeviceConfigurationService" />
		</wsdl:port>
	</wsdl:service>
