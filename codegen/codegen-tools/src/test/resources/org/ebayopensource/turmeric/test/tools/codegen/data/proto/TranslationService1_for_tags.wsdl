			<xs:annotation>
				<xs:documentation>
					Unique identifier for a realm.
				</xs:documentation>
				<xs:appinfo>
					<CallInfo>
						<CallName>getRealms</CallName>
						<Returned>Always</Returned>
					</CallInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element name="realmName" type="xs:string">
			<xs:annotation>
				<xs:documentation>
					Name of the realm.
				</xs:documentation>
				<xs:appinfo>
					<MaxLength>256</MaxLength>
					<CallInfo>
						<CallName>getRealms</CallName>
						<Returned>Always</Returned>
					</CallInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element name="parent" minOccurs="0" type="tns:Realm">
			<xs:annotation>
				<xs:documentation>
					Parent of the realm.
				</xs:documentation>
				<xs:appinfo>
					<CallInfo>
						<CallName>getRealms</CallName>
						<Returned>Conditionally</Returned>
					</CallInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element name="child" maxOccurs="unbounded" minOccurs="0" type="tns:Realm">
			<xs:annotation>
				<xs:documentation>
					Children of the realm.
				</xs:documentation>
				<xs:appinfo>
					<CallInfo>
						<CallName>getRealms</CallName>
						<Returned>Conditionally</Returned>
					</CallInfo>
				</xs:appinfo>
