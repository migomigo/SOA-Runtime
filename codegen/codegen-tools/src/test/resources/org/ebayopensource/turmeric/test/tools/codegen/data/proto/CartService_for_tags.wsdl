			<xs:element name="initPaymentRequest" type="tns:InitPaymentRequest" />

			<xs:complexType name="InitPaymentRequest">
				<xs:annotation>
					<xs:documentation>
						Starts a session with the Payment service. 
						This request takes several input parameters.
						The request parameters are: 
						(a) Cart Identifier
						(b) Billing address broken down into component parts
						(c) Return URL to navigate back
						(d) Error URL to redirect to in case of an error
						(e) Payment method
						(f) Optional Payment method parameters
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseCartRequest">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="1" name="cartIdentifier"
								type="xs:string">
								<xs:annotation>
									<xs:documentation>
										The ID of the cart.
									</xs:documentation>							
								</xs:annotation>
							</xs:element>
							<xs:element name="billingAddress" type="tns:Address"
								maxOccurs="1" minOccurs="0">
								<xs:annotation>
									<xs:documentation>
										The billing address of the cart.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="1" name="returnURL" type="xs:string">

								<xs:annotation>
									<xs:documentation>
										The return redirect URL for the Payment.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="1" name="errorURL" type="xs:string">

								<xs:annotation>
									<xs:documentation>
										The redirect URL in case an error occurs during request processing.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="1" name="paymentMethod"
								type="xs:string">
									<xs:annotation>
										<xs:documentation>
											Indicates the chosen payment method.
										</xs:documentation>
									</xs:annotation>
							</xs:element>











							<xs:element name="paymentMethodParameters" type="tns:PaymentMethodParameterList"
								maxOccurs="1" minOccurs="0">
							</xs:element>

						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>


			<xs:element name="initPaymentResponse" type="tns:InitPaymentResponse" />			

