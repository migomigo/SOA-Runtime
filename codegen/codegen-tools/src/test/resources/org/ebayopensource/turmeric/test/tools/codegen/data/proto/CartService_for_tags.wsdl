			<xs:complexType name="SalesTax">
				<xs:annotation>
					<xs:documentation>
						(US only) The amount of sales tax, calculated
						for a transaction based on the SalesTaxPercent
						and pricing information.
			</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="BuyingCommonTypeLibrary"
							namespace="http://www.ebay.com/marketplace/buying/v1/services/types"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="0" name="salesTaxAmount" type="tns:Amount">
						<xs:annotation>
							<xs:documentation>
								The amount of sales tax, calculated for
								a transaction based on the
								SalesTaxPercent and pricing information.
								(US only)
					</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>getCheckoutCart</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="salesTaxPercent" type="xs:float">
						<xs:annotation>
							<xs:documentation>
								Percent of an item's price to be charged
								as the sales tax for the transaction.
								The input value is stored with a
								precision of 3 digits after the decimal
								point (##.###).
					</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>getCheckoutCart</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="salesTaxState" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								State or jurisdiction for which the
								sales tax is being collected. Only
								returned if the seller specified a
								value.
					</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>getCheckoutCart</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="shippingIncludedInTax"
						type="xs:boolean">
						<xs:annotation>
							<xs:documentation>
								Whether shipping costs were part of the
								base amount that was taxed. Flat or
								calculated shipping. (US only)
					</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>getCheckoutCart</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
