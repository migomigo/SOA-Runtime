				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="0" name="name" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								Name associated with address, if such
								exists.
					</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>getCheckoutCart</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="street1" type="xs:string">
						<xs:annotation>
							<xs:documentation>
