									<xs:documentation>
										The generated token for the payment.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="operationErrors" type="tns:ErrorMessage"
								minOccurs="0">
							</xs:element>

						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>



