			<xs:complexType name="LineItem">
				<xs:annotation>
					<xs:documentation>
						An entity or line item.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="1"
						name="cartLineItemIdentifier" type="xs:string">
