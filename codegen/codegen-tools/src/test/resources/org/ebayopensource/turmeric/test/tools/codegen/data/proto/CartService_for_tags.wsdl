							<xs:documentation>
								The carrier name for the shipping service.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1"
						name="shippingServiceName" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								The name to be displayed for this
								shipping
								service.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>getCheckoutCart</CallName>
									<Returned>Always</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingServiceToken" type="xs:string"
						maxOccurs="1" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								The token for the shipping service.
							</xs:documentation>
						</xs:annotation>
