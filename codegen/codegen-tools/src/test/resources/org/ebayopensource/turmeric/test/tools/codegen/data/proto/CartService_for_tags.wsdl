						<xs:annotation>
							<xs:documentation>
								The total of the amounts that the buyer
								saved on a collection of items as
								specified by the shipping engine rules.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="total" type="tns:Amount"
						maxOccurs="1" minOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The total amount value.
							</xs:documentation>
						</xs:annotation>
					</xs:element>				</xs:sequence>
			</xs:complexType>

