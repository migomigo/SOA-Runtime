								<ListBasedOn>
									PaymentStatusList
								</ListBasedOn>
								<CallInfo>
									<CallName>getCheckoutCart</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>

					</xs:element>
					<xs:element name="purchaseId" type="xs:string"
						maxOccurs="1" minOccurs="0"></xs:element>
				</xs:sequence>
			</xs:complexType>
