			<xs:simpleType name="ISOCountryCodeType">
				<xs:annotation>
					<xs:documentation>
						Two-letter country code, according to ISO 3166-1 alpha-2 standard
						ISO 3166 standard defines three sets of country codes
						1) ISO 3166-1 alpha-2 - two-letter country codes
						2) ISO 3166-1 alpha-3 - three-letter country codes
						3) ISO 3166-1 numeric - three-degit country codes
					</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource
							library="PaymentDomainTypeLibrary"
							namespace="http://www.ebay.com/marketplace/payment/v1/services">
						</typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:restriction base="xs:string">
					<xs:pattern value="[a-zA-Z][a-zA-Z]"></xs:pattern>
				</xs:restriction>
			</xs:simpleType>

