	<portType name="ResolutionCaseManagementServicePort">
		<operation name="getVersion">
			<xs:documentation>
				Get current service version.
			</xs:documentation>
			<input message="tns:getVersionRequest"/>
			<output message="tns:getVersionResponse"/>
		</operation>
		<operation name="getUserCases">
			<xs:documentation>
				Get list of resolution cases for an eBay member. This includes cases from both the
				new eBay Buyer Protection(eBP) and the eBay and PayPal ODR systems.
			</xs:documentation>
			<input message="tns:getUserCasesRequest"/>
			<output message="tns:getUserCasesResponse"/>
		</operation>
		<operation name="getEBPCaseDetail">
			<xs:documentation>
				Gets details on a specific eBay Buyer Protection (eBP) case.
			</xs:documentation>
			<input message="tns:getEBPCaseDetailRequest"/>
			<output message="tns:getEBPCaseDetailResponse"/>
		</operation>
		<operation name="getActivityOptions">
			<xs:documentation>
				Get activity options for the current state of the eBay Buyer Protection (eBP) case.
			</xs:documentation>
			<input message="tns:getActivityOptionsRequest"/>
			<output message="tns:getActivityOptionsResponse"/>
		</operation>
		<operation name="issueFullRefund">
			<xs:documentation>
				Issues full refund.
			</xs:documentation>
			<input message="tns:issueFullRefundRequest"/>
			<output message="tns:issueFullRefundResponse"/>
		</operation>
		<operation name="provideTrackingInfo">
			<xs:documentation>
				Accepts the tracking info and associate the tracking info to the case.
			</xs:documentation>
			<input message="tns:provideTrackingInfoRequest"/>
			<output message="tns:provideTrackingInfoResponse"/>
		</operation>		
		<operation name="sellerOfferAnotherSolution">
			<xs:documentation>
				Accepts the alternate solution from Seller and inform buyer.
			</xs:documentation>
			<input message="tns:sellerOfferAnotherSolutionRequest"/>
			<output message="tns:sellerOfferAnotherSolutionResponse"/>
		</operation>
		<operation name="escalateToCustomerSupport">
			<xs:documentation>
				Esclate the case to customer support.
			</xs:documentation>
			<input message="tns:escalateToCustomerSupportRequest"/>
			<output message="tns:escalateToCustomerSupportResponse"/>
		</operation>
		<operation name="appealToCustomerSupport">
			<xs:documentation>
				Appeal the case to customer support.
			</xs:documentation>
			<input message="tns:appealToCustomerSupportRequest"/>
			<output message="tns:appealToCustomerSupportResponse"/>
		</operation>
		<operation name="getEBPMetadata">
			<xs:documentation>
				Fetches the meta data informations associated to different types.
			</xs:documentation>
			<input message="tns:getEBPMetadataRequest"/>
			<output message="tns:getEBPMetadataResponse"/>
		</operation>
	</portType>

