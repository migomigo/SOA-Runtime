<xs:complexType name="EBPCaseDetailType">
	<xs:annotation>
		<xs:documentation>
			Contains the details of a single eBP case.
		</xs:documentation>
	</xs:annotation>
	<xs:sequence>
		<xs:element minOccurs="0" name="openReason" type="xs:token">
			<xs:annotation>
				<xs:documentation>
					For future use.
				</xs:documentation>
				<xs:appinfo>
					<tagStatus>ComingSoon</tagStatus>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element minOccurs="0" name="decisionReason" type="xs:token">
			<xs:annotation>
				<xs:documentation>
					The latest decision reason code associated with
					case. Usually the case closure decision reason.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element minOccurs="0" name="decisionDate" type="xs:dateTime">
			<xs:annotation>
				<xs:documentation>
					The date when a decision was made and the case was
					closed.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>


		<xs:element minOccurs="0" name="decision" type="tns:CaseDecisionType">
			<xs:annotation>
				<xs:documentation>
					The disposition of the case.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element maxOccurs="1" minOccurs="0" name="FVFCredited" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>
					Indicates whether or not a Final Value Fee (FVF) was
					credited as part of the case decision.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element maxOccurs="1" minOccurs="0" name="notCountedInBuyerProtectionCases" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>
					If false, this case was counted against seller's
					performance.  If true, this case was not counted against seller's performance.  Tag is not returned if not applicable.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element name="globalId" type="xs:token">
			<xs:annotation>
				<xs:documentation>
					The eBay site where case was filed.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Always</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element maxOccurs="unbounded" minOccurs="0" name="appeal" type="tns:AppealType">
			<xs:annotation>
				<xs:documentation>
					Indicates whether the appeal originated from a buyer
					or seller.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element maxOccurs="unbounded" minOccurs="0" name="responseHistory" type="tns:EBPCaseResponseHistoryType">
			<xs:annotation>
				<xs:documentation>
					The history of responses for the case.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element maxOccurs="1" minOccurs="0" name="agreedRefundAmount" type="tns:Amount">
			<xs:annotation>
				<xs:documentation>
					In the case of a partial or full refund, this value
					indicates the monetary amount of the refund.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
			</xs:element>
		<xs:element minOccurs="0" name="paymentDetail" type="tns:PaymentDetailType">
			<xs:annotation>
				<xs:documentation>
					Money movement details.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element minOccurs="0" name="buyerReturnShipment" type="tns:ShipmentType">
			<xs:annotation>
				<xs:documentation>
					In the case of a return, this field contains details
					on the buyer's return shipment.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element name="sellerShipment" type="tns:ShipmentType">
			<xs:annotation>
				<xs:documentation>
					The details on the seller's shipment.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Always</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element name="detailStatus" type="xs:token">
			<xs:annotation>
				<xs:documentation>
					Additional status information.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Always</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element name="initialBuyerExpectation" type="xs:token">
			<xs:annotation>
				<xs:documentation>
					Initial buyer expectation.
				</xs:documentation>
				<xs:appinfo>
					<callInfo>
						<callName>getEBPCaseDetail</callName>
						<returned>Always</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>

	</xs:sequence>
</xs:complexType>


<xs:complexType name="PaymentDetailType">
