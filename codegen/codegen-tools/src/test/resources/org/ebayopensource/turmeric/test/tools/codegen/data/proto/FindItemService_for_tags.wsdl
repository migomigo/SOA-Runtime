								</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element maxOccurs="1" minOccurs="0" name="moneyOperand" type="tns:MoneyOperand">
							<xs:annotation>
								<xs:documentation>
									operand of money type, e.g. GT(CurrentPrice, 100) 	
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element maxOccurs="1" minOccurs="0" name="tokenOperand" type="tns:TokenOperand">
							<xs:annotation>
								<xs:documentation>
