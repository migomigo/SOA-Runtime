								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="1" minOccurs="0" name="dateOperand" type="tns:DateOperand">
							<xs:annotation>
								<xs:documentation>
									operand of date type, e.g. LE(EndTime, 20091225)	
								</xs:documentation>
							</xs:annotation>
						</xs:element>						
						<xs:element maxOccurs="1" minOccurs="0" name="expressionOperand" type="tns:SimpleNumericExpression">
							<xs:annotation>
								<xs:documentation>
									operand of expression type, e.g. LE(EndTime, ADD(NOW,20))	
								</xs:documentation>
							</xs:annotation>						
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>				
		</xs:complexContent>
