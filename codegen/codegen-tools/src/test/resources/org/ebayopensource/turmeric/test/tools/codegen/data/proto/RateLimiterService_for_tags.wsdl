            <xs:complexType name="isRateLimitedRequest">
				<xs:annotation>
					<xs:documentation>
						Contains all the information which related to rate limiting
					</xs:documentation>
					<xs:appinfo>
						<Summary>
							if the requested subject/subjectGroup has been rate limited for the resource/operation
						</Summary>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseRateLimiterServiceRequest">
						<xs:sequence>
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="subject" type="tns:SubjectType" >
								<xs:annotation>
									<xs:documentation>
										Contains subject information for rate limiting request
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>isRateLimited</CallName>
											<RequiredInput>Yes</RequiredInput>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>

							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="resolvedSubjectGroup" type="tns:SubjectGroupType" >
								<xs:annotation>
									<xs:documentation>
									    Contains subject groups which has group membership resolved
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>isRateLimited</CallName>
											<RequiredInput>No</RequiredInput>
										</CallInfo>
										<CallInfo>
											<CallName>isRateLimited</CallName>
											<Returned>Always</Returned>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>

							<xs:element name="challengeAnswer" type="tns:ChallengeAnswer"
								maxOccurs="1" minOccurs="0">
								<xs:annotation>
									<xs:documentation>
										contains information for challenge answer
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>isRateLimited</CallName>
											<RequiredInput>No</RequiredInput>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>

							<xs:element name="attr" type="tns:NameValuePair" maxOccurs="unbounded" minOccurs="0">
								<xs:annotation>
									<xs:documentation>
										contains addition information to do rate limiter
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>isRateLimited</CallName>
											<RequiredInput>No</RequiredInput>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
            </xs:complexType>

            <xs:complexType name="isRateLimitedResponse">
				<xs:annotation>
					<xs:documentation>
						contains information of rate limiter status, or challenge information if the status is "Challenge"
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseRateLimiterServiceResponse">
						<xs:sequence>
							<xs:element minOccurs="0" name="status"
								type="tns:RateLimiterStatus" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>
										status of the Rate limiting
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>isRateLimited</CallName>
											<Returned>Always</Returned>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>

							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="resolvedSubjectGroup" type="tns:SubjectGroupType" >
								<xs:annotation>
									<xs:documentation>
										contains subject group list which has group membership resolved.
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>isRateLimited</CallName>
											<Returned>Conditionally</Returned>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>

							<xs:element name="challenge" type="tns:Challenge" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>
										contains information for answering challenge question.
										Returned only if status is SERVE_GIF
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>isRateLimited</CallName>
											<Returned>Conditionally</Returned>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>

			<xs:simpleType name="RateLimiterStatus">
				<xs:annotation>
					<xs:documentation>
						return the Rate Limiter Status
					</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:string">
					<xs:enumeration value="SERVE_OK">
						<xs:annotation>
							<xs:documentation>
								No Block from Rate Limiter
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="FLAG">
						<xs:annotation>
							<xs:documentation>
								Flaged by Rate Limiter
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="SERVE_GIF">
						<xs:annotation>
							<xs:documentation>
								Server Gif by Rate Limiter, to continue the call, the caller need to resolved the Challenge question
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="BLOCK">
						<xs:annotation>
							<xs:documentation>
								Block by Rate Limiter
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="RequestHasBeenServed">
						<xs:annotation>
							<xs:documentation>
								Request Has Been Served by Rate Limiter
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Invalid">
						<xs:annotation>
							<xs:documentation>
								Invalid
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>

