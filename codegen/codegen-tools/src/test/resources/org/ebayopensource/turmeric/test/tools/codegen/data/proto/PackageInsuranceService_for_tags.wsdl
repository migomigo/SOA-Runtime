<?xml version="1.0" encoding="UTF-8"?>
<definitions name="PackageInsuranceService"
	targetNamespace="http://www.ebay.com/marketplace/shipping/v1/services"
	xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/"
	xmlns:tns="http://www.ebay.com/marketplace/shipping/v1/services"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/">
	<types>
		<xs:schema attributeFormDefault="unqualified"
			elementFormDefault="qualified"
			targetNamespace="http://www.ebay.com/marketplace/shipping/v1/services"
			xmlns:tns1="http://www.ebay.com/marketplace/shipping/v1/services">
			<xs:element name="createClaimResponse" type="tns:CreateClaimResponse" />
			<xs:element name="createClaimRequest" type="tns:CreateClaimRequest" />
			<xs:element name="confirmClaimResponse" type="tns:ConfirmClaimResponse" />
			<xs:element name="confirmClaimRequest" type="tns:ConfirmClaimRequest" />
			<xs:element name="purchaseResponse" type="tns:PurchaseResponse" />
			<xs:element name="purchaseRequest" type="tns:PurchaseRequest" />
			<xs:element name="purchaseInitResponse" type="tns:PurchaseInitResponse" />
			<xs:element name="purchaseInitRequest" type="tns:PurchaseInitRequest" />
			<xs:element name="getClaimResponse" type="tns:GetClaimResponse" />
			<xs:element name="getClaimRequest" type="tns:GetClaimRequest" />
			<xs:element name="getQuoteResponse" type="tns:GetQuoteResponse" />
			<xs:element name="getQuoteRequest" type="tns:GetQuoteRequest" />
			<xs:simpleType name="AddressType">
				<xs:annotation>
					<xs:documentation>
						RESIDENTIAL,COMMERCIAL,UNKNOWN - Default
					</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="ShippingDomainTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:restriction base="xs:token"></xs:restriction>
			</xs:simpleType>
			<xs:complexType name="Amount">
				<xs:annotation>
					<xs:appinfo>
						<typeLibrarySource library="MarketPlaceServiceCommonTypeLibrary"
							namespace="http://www.ebay.com/marketplace/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleContent>
					<xs:extension base="xs:double">
						<xs:attribute name="currencyId" type="xs:string" use="required"></xs:attribute>
					</xs:extension>
				</xs:simpleContent>
			</xs:complexType>
			<xs:complexType abstract="true" name="BaseRequest">
				<xs:annotation>
					<xs:documentation>
						This is the base class for request container for
						all
						service operations.
			</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="MarketPlaceServiceCommonTypeLibrary"
							namespace="http://www.ebay.com/marketplace/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="extension"
						type="tns:ExtensionType"></xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="ClaimDetail">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="0" name="claimCreationDate" type="xs:dateTime"></xs:element>
					<xs:element minOccurs="0" name="claimId" type="xs:long"></xs:element>
					<xs:element minOccurs="0" name="providerClaimId" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="providerClaimDate" type="xs:dateTime"></xs:element>
					<xs:element minOccurs="0" name="claimReason" type="xs:integer"></xs:element>
					<xs:element minOccurs="0" name="claimStatus" type="xs:token"></xs:element>
					<xs:element minOccurs="0" name="claimSettlementAmount"
						type="tns:Amount"></xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="ConfirmClaimRequest">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:PackageInsuranceBaseRequest">
						<xs:sequence>
							<xs:element minOccurs="1" name="insuranceId" type="xs:long"></xs:element>
							<xs:element minOccurs="1" name="claimId" type="xs:long"></xs:element>
							<xs:element minOccurs="0" name="confirmReason" type="xs:token">
								<xs:annotation>
									<xs:documentation>
										aka: buyerClaimType.
							</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0" name="subReason" type="xs:token"></xs:element>
							<!--
								discuss with Maulin, delete <xs:element name="consigneeAddress"
								type="xs:consigneeAddress" minOccurs="0" />
							-->
							<xs:element minOccurs="0" name="confirmMethod" type="xs:token">
								<xs:annotation>
									<xs:documentation>
										aka: verificationTypeEnum.
							</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="ConfirmClaimResponse">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseResponse">
						<xs:sequence>
							<xs:element minOccurs="0" name="insuranceId" type="xs:long"></xs:element>
							<xs:element minOccurs="0" name="confirmationId" type="xs:string">
								<xs:annotation>
									<xs:documentation>
										TP_CLAIM_INFO.CLAIM_VERIFICATION_ID
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0" name="confirmationDate"
								type="xs:dateTime">
								<xs:annotation>
									<xs:documentation>
										TP_CLAIM_INFO.CLAIM_VERIFICATION_DATE
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="ContactAddress">
				<xs:annotation>
					<xs:documentation>Postal mail address</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="ShippingDomainTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="0" name="addressLine1" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="addressLine2" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="addressLine3" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="suiteOrApt" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="city" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="stateOrProvince" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="postalCode" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="additionalPostalCode"
						type="xs:string"></xs:element>
					<xs:element minOccurs="1" name="country" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="urbancode" type="xs:string"></xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="ContactInfo">
				<xs:annotation>
					<xs:documentation>
						Detailed Contact information - Contact name,
						address and
						phone number
			</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="ShippingDomainTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="0" name="firstName" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="middleName" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="lastName" type="xs:string"></xs:element>
					<xs:element minOccurs="1" name="contactAddress" type="tns:ContactAddress"></xs:element>
					<xs:element minOccurs="0" name="companyName" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="phone" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="extension" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="emailAddress" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="phone2" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="fax" type="xs:string"></xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="CostBreakUp">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="ShippingDomainTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="1" name="name"
						type="xs:token"></xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="value"
						type="xs:string"></xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="cost"
						type="tns:Amount"></xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="CreateClaimRequest">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:PackageInsuranceBaseRequest">
						<xs:sequence>
							<xs:element minOccurs="1" name="insuranceId" type="xs:long"></xs:element>
							<xs:element minOccurs="0" name="insurancePolicyId"
								type="xs:string"></xs:element>
							<xs:element minOccurs="0" name="claimReason" type="xs:token"></xs:element>
							<xs:element minOccurs="0" name="trackingNumber" type="xs:string">
								<xs:annotation>
									<xs:documentation>
										In create claim they need to have this,
										in
										case of eBay, we can derive.
							</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0" name="paypalEmail" type="xs:string"></xs:element>
							<xs:element minOccurs="0" name="claimAmount" type="tns:Amount">
								<xs:annotation>
									<xs:documentation>
										Default value is insured value.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="GetClaimRequest">
				<xs:annotation>
					<xs:documentation>
						This call will return all claims for a given
						tpInsuranceId, if claimId is not provided, it will
						return all
						claims.
			</xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:PackageInsuranceBaseRequest">
						<xs:sequence>
							<xs:element minOccurs="1" name="insuranceId" type="xs:long"></xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="0" name="claimId"
								type="xs:string"></xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="GetClaimResponse">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseResponse">
						<xs:sequence>
							<xs:element minOccurs="0" name="insuranceId" type="xs:long"></xs:element>
							<xs:element maxOccurs="unbounded" name="claimDetail"
								type="tns:ClaimDetail"></xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="GetQuoteRequest">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseRequest">
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="1"
								name="insuranceProvider" type="xs:token">
								<xs:annotation>
									<xs:documentation>
										Override the base to make it required.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="1" name="insuredAmount" type="tns:Amount"></xs:element>
							<xs:element minOccurs="0" name="effectiveDate" type="xs:dateTime"></xs:element>
							<xs:element maxOccurs="unbounded" name="additionalOption"
								type="tns:ShippingOption">
								<xs:annotation>
									<xs:documentation>
										eBay specific name value pairs, eg:
										quantity.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="GetQuoteResponse">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseResponse">
						<xs:sequence>
							<xs:element minOccurs="0" name="quote" type="tns:Quote"></xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="PackageInsuranceBaseRequest">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseRequest">
						<xs:sequence>
							<!--
								tbd: madhu to decide how the auth container should look like
								<xs:element name="ieClientEnum" type="xs:token" minOccurs="1"
								maxOccurs="1" /> <xs:element name="userName" type="xs:string"
								minOccurs="0" /> <xs:element name="password" type="xs:string"
								minOccurs="0" /> <xs:element name="userId" type="xs:long"
								minOccurs="0" />
							-->
							<xs:element maxOccurs="1" minOccurs="0"
								name="insuranceProvider" type="xs:token">
								<xs:annotation>
									<xs:documentation>
										Not required (minOccurs=&quot;0&quot;) if
										Create.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="1" name="externalRefId" type="xs:long">
								<xs:annotation>
									<xs:documentation>
										Also called clientTxnId, caller's reference
										ID,
										in label it is
										the Shipment ID.
							</xs:documentation>
								</xs:annotation>
							</xs:element>
							<!--
								we need to figure out how to make this an option only ebay
								client will provide <xs:element name="thirdPartySiteId"
								type="xs:long" minOccurs="0" />
							-->
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="additionalOption" type="tns:ShippingOption">
								<xs:annotation>
									<xs:documentation>
										eBay specific name value pairs, eg:
										thirdPartySiteId, transationTime.
							</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="PackageInsuranceContent">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="1" name="referenceId" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="description" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="categoryPath" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="accessUrl" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="price" type="tns:Amount"></xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="PackageInsuranceInfo">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="0" name="insuranceId" type="xs:long"></xs:element>
					<!-- tbd -->
					<xs:element minOccurs="0" name="shippingCarrier" type="xs:token"></xs:element>
					<!-- tbd -->
					<xs:element minOccurs="0" name="shippingService" type="tns:ShippingService"></xs:element>
					<xs:element minOccurs="0" name="shippingDate" type="xs:dateTime"></xs:element>
					<!-- tbd -->
					<xs:element minOccurs="0" name="legalPolicyHolderInfo"
						type="tns:ContactInfo"></xs:element>
					<xs:element maxOccurs="unbounded" name="packageInsuranceContent"
						type="tns:PackageInsuranceContent"></xs:element>
					<xs:element minOccurs="0" name="shipToAddress" type="tns:ContactAddress"></xs:element>
					<xs:element minOccurs="0" name="shipFromAddress" type="tns:ContactAddress"></xs:element>
					<xs:element minOccurs="0" name="quote" type="tns:Quote"></xs:element>
					<!-- tbd -->
					<xs:element minOccurs="0" name="insuredValue" type="tns:Amount"></xs:element>
					<xs:element minOccurs="0" name="orderValue" type="tns:Amount"></xs:element>
					<xs:element minOccurs="0" name="policyID" type="xs:string"></xs:element>
					<xs:element minOccurs="0" name="policyDate" type="xs:dateTime"></xs:element>
					<xs:element minOccurs="0" name="fromZipCode" type="xs:string"></xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="PurchaseInitRequest">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:PackageInsuranceBaseRequest">
						<xs:sequence>
							<xs:element minOccurs="0" name="transactionTime" type="xs:dateTime"></xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="PurchaseInitResponse">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseResponse">
						<xs:sequence>
							<xs:element minOccurs="0" name="tpInsuranceId" type="xs:long"></xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="PurchaseRequest">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:PackageInsuranceBaseRequest">
						<xs:sequence>
							<xs:element minOccurs="1" name="insuranceId" type="xs:long"></xs:element>
							<xs:element minOccurs="0" name="trackingNumber" type="xs:string"></xs:element>
							<xs:element minOccurs="1" name="insuranceInfo"
								type="tns:PackageInsuranceInfo"></xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="PurchaseResponse">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseResponse">
						<xs:sequence>
							<xs:element minOccurs="1" name="tpInsuranceId" type="xs:long"></xs:element>
							<xs:element minOccurs="0" name="policyID" type="xs:string"></xs:element>
							<xs:element minOccurs="0" name="policyDate" type="xs:dateTime"></xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="Quote">
				<xs:annotation>
					<xs:documentation></xs:documentation>
					<xs:appinfo>
						<typeLibrarySource library="PackageInsuranceTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services"></typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="0" name="quoteId" type="xs:long"></xs:element>
					<xs:element minOccurs="1" name="totalPremium" type="tns:Amount"></xs:element>
					<xs:element maxOccurs="unbounded" name="costBreakup"
						type="tns:CostBreakUp"></xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="ShippingOption">
