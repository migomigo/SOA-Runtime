			<!-- Added as part of Critical API Enhancements_24858   ************End************-->
			<!--  End Imported Types -->
			<!-- Basic complex types -->
			<xs:complexType name="TransactionInformation">
				<xs:annotation>
					<xs:documentation>
						Identifies an eBay Transaction information.
						Should include both eBay Item id and eBay
						Transaction Id.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="transactionId"
						type="tns:TransactionId" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The transaction id associated with an
								eBay purchase.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="itemId" type="tns:ItemId"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The eBay item id.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>


			<xs:complexType name="ShipmentTrackingResponseInfo">
				<xs:annotation>
					<xs:documentation>
						A single response object.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="shipmentTrackingDetail"
						type="tns:ShipmentTrackingDetail" minOccurs="0"
						maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
								Provides the tracking details.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="shipmentTrackingTransactionalInfo"
						type="tns:ShipmentTrackingTransactionalInfo" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Transactional info for the shipment
								tracking.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="correlationId" type="xs:string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								A unique id identifying the particular
								request.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="errorMessage"
						type="tns:ErrorMessage" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
								Error Information.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="ShipmentTrackingRequestInfo">
				<xs:annotation>
					<xs:documentation>
						A single request object.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="shipmentTracking"
						type="tns:ShipmentTracking" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The shipping carrier of the tracking
								number.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="correlationId" type="xs:string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								A unique id identifying the particular
								request.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="shipmentTrackingTransactionalInfo"
						type="tns:ShipmentTrackingTransactionalInfo" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								A unique id identifying the particular
								request.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="ShipmentTrackingRequest">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest">
						<xs:sequence>
							<xs:element
								name="shipmentTrackingRequestInfo"
								type="tns:ShipmentTrackingRequestInfo" minOccurs="1"
								maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>
										Request for the shipment
										tracking operations.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sellerId"
								type="xs:long">
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="ShipmentTrackingResponse">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse">
						<xs:sequence>
							<xs:element
								name="shipmentTrackingResponseInfo"
								type="tns:ShipmentTrackingResponseInfo" minOccurs="1"
								maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>
										Response for the shipment
										tracking operations.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>



			<!--  Elements for Request / Response -->
			<xs:element name="getVersionRequest" 
				type="tns:GetVersionRequest" />
			<xs:element name="getVersionResponse" 
				type="tns:GetVersionResponse" />
			<xs:element name="shipmentTrackingRequest"
				type="tns:ShipmentTrackingRequest">
				<xs:annotation>
					<xs:documentation>
						Request for the shipment tracking operations. A
						request having one or more
						ShipmentTrackingSingleRequest objects.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shipmentTrackingResponse"
				type="tns:ShipmentTrackingResponse">
				<xs:annotation>
					<xs:documentation>
						Response for the shipment tracking operations. A
						response can have one or more
						ShipmentTrackingSingleResponse objects.
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<!-- Added as part of Critical API Enhancements_24858   ************Start************-->
			<xs:element name="getShippingCostResponse"
				type="tns:GetShippingCostResponse">
				<xs:annotation>
					<xs:documentation>
						Response for the GetShippingCost operations.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="getShippingCostRequest"
				type="tns:GetShippingCostRequest">
				<xs:annotation>
					<xs:documentation>
						Request for the GetShippingCost operations.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Added as part of Critical API Enhancements_24858   ************End************-->

			<!--  End Elements -->
			<!-- Types for Add Tracking -->
			<xs:complexType name="GetVersionRequest">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceRequest" />
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="GetVersionResponse">
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse" />
				</xs:complexContent>
			</xs:complexType>


			<xs:complexType name="ShipmentTracking">
				<xs:sequence>
					<xs:element name="shippingCarrier"
						type="tns:ShippingCarrier" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Name of the Carrier for which tracking
								number is being Added (UPS,USPS, FedEx
								etc)
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="notes" type="xs:string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Custom Notes that seller can add to
								tracking number. (for eBay internal use
								only)
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="trackingNumber"
						type="tns:TrackingNumber" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Shipment Tracking Number.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="shipmentId" type="xs:long"></xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="ShipmentTrackingDetail">
				<xs:sequence>
					<xs:element name="tracking"
						type="tns:ShipmentTracking" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Details of the Shipment Tracking.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="trackingStatus" type="xs:string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The current status of the tracking
								number. Valid values are
								SHIPMENT_CREATED, DROPPED_OFF,
								IN_TRANSIT, DELIVERED, RETURNED,
								CANCELLED, LABEL_PRINTED, ERROR,
								UNCONFIRMED, PICKUP_FAILED,
								DELIVERY_DELAYED, DELIVERY_SCHEDULED,
								DELIVERY_FAILED, INRETURN.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="history"
						type="tns:ShipmentTrackingHistory" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Shipment Tracking History.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="eBayPrintedLabel"
						type="xs:boolean">
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="ShipmentTrackingHistory">
				<xs:sequence>
					<xs:element name="history" type="xs:string"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								the tracking history associated with a
								tracking number.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="ShipmentTrackingTransactionalInfo">
				<xs:sequence>
					<xs:element name="orderId" type="tns:OrderId"
						minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								eBay OrderId.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="transactionInformation"
						type="tns:TransactionInformation" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								the eBay tracking Info.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<!-- Added as part of Critical API Enhancements_24858   ************Start************-->
			<xs:complexType name="DiscountInfo">
				<xs:annotation>
					<xs:appinfo>
						<typeLibrarySource
							library="ShippingDomainTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services">
						</typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="intlDiscountProfileId"
						type="xs:long" minOccurs="0" maxOccurs="1">
					</xs:element>
					<xs:element name="applyDiscountFlag"
						type="xs:boolean" minOccurs="0" maxOccurs="1">
					</xs:element>
					<xs:element name="domesticDiscountProfileId"
						type="xs:long" minOccurs="0" maxOccurs="1">
					</xs:element>
					<xs:element name="intlShippingDiscountVersionId"
						type="xs:long" minOccurs="0" maxOccurs="1">
					</xs:element>
					<xs:element name="domesticPromoShipping"
						type="xs:boolean" minOccurs="0" maxOccurs="1">
					</xs:element>
					<xs:element name="domesticShippingDiscountVersionId"
						type="xs:long" minOccurs="0" maxOccurs="1">
					</xs:element>
					<xs:element name="intlPromoShipping"
						type="xs:boolean" minOccurs="0" maxOccurs="1">
					</xs:element>
					<xs:element name="domesticAdditionalProfile"
						type="xs:boolean" minOccurs="0" maxOccurs="1" />
					<xs:element name="intlAdditionalProfile"
						type="xs:boolean" minOccurs="0" maxOccurs="1" />

						processContents="lax">

				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="FreightShipping">
				<xs:annotation>
					<xs:appinfo>
						<typeLibrarySource
							library="ShippingDomainTypeLibrary"
							namespace="http://www.ebay.com/marketplace/shipping/v1/services">
						</typeLibrarySource>
					</xs:appinfo>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="freightPackagingHelpRequired"
						type="xs:boolean" minOccurs="0" maxOccurs="1">
					</xs:element>
					<xs:element name="freightPickUpLocationType"
						type="xs:token">
					</xs:element>
					<xs:element name="freightEntryPermissionRequired"
						type="xs:boolean">
