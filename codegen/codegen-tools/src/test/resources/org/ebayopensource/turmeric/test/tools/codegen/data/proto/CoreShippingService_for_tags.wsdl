					<xs:enumeration value="FIM" /><!-- Finnish Markka                    Finland                         246               -->
					<xs:enumeration value="GRD" /><!-- Greek Drachma                     Greece                          300               -->
					<xs:enumeration value="HKD" /><!-- Hong Kong Dollar                  Hong Kong                       344               -->
