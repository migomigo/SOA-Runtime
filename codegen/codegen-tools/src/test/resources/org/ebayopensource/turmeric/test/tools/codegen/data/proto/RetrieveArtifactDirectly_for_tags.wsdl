			<xsd:simpleType name="AckValue">
				<xsd:annotation>
					<xsd:documentation>
						Indicates whether the error is a severe error (causing the request to fail)
						or an informational error (a warning) that should be communicated
						to the
						user.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="MarketPlaceServiceCommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:restriction base="xsd:string">

					<xsd:enumeration value="Success">
						<xsd:annotation>
							<xsd:documentation>
								The request was processed successfully, but something occurred that
								may
								affect your application or the user.				
					</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="Failure">
						<xsd:annotation>

							<xsd:documentation>
								The request that triggered the error was not processed
								successfully.
								When a serious application-level error occurs, the error is returned
								instead of the business data.
					</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="Warning">
						<xsd:annotation>
							<xsd:documentation>
								The request that triggered the error was processed successfully but
								with some warnings.
					</xsd:documentation>

						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="PartialFailure">
						<xsd:annotation>
							<xsd:documentation>
								The request that triggered the error was processed successfully but
								with some warnings.				</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>

				</xsd:restriction>
			</xsd:simpleType>
			<xsd:complexType abstract="true" name="BaseServiceResponse">
				<xsd:annotation>
					<xsd:documentation>
						Base response container for all service operations.
						Contains error information associated with the request.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="MarketPlaceServiceCommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>

				<xsd:sequence>
					<xsd:element name="ack" type="tns:AckValue">
						<xsd:annotation>
							<xsd:documentation>
                             Indicates whether or not errors or warnings were 
                             generated during the processing of the request.
							</xsd:documentation>
							<xsd:appinfo>
								<CallInfo>
									<AllCalls/>
									<Returned>Always</Returned>
								</CallInfo>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					
					<xsd:element minOccurs="0" name="errorMessage" type="tns:ErrorMessage">
						<xsd:annotation>
							<xsd:documentation>
								Information for an error or warning that
								occurred when eBay processed the request.
					        </xsd:documentation>
							<xsd:appinfo>
								<CallInfo>
									<AllCalls/>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>

					<xsd:element minOccurs="0" name="version" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>
							The released version of this service that eBay uses to process the request.
							The version in use is normally the latest release version,
		  					as specified in the service documentation.							
							</xsd:documentation>
							<xsd:appinfo>
								<CallInfo>
									<AllCalls/>
									<Returned>Always</Returned>

								</CallInfo>
							</xsd:appinfo>
						</xsd:annotation>

					</xsd:element>
					<xsd:element minOccurs="0" name="timestamp" type="xsd:dateTime">
						<xsd:annotation>
							<xsd:documentation>
							This value represents the date and time when eBay processed the request.
							This value is returned in GMT, the ISO 8601 date and time format 
							(YYYY-MM-DDTHH:MM:SS.SSSZ). See the &quot;dateTime&quot; type for information 
							about the time format, and for details on converting to and from the GMT time zone.
							</xsd:documentation>

							<xsd:appinfo>
								<CallInfo>
									<AllCalls/>
									<Returned>Always</Returned>
								</CallInfo>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					
				</xsd:sequence>
			</xsd:complexType>
			<xsd:simpleType name="ErrorCategory">
				<xsd:annotation>
					<xsd:documentation>
						There are three categories of errors: Request errors, Application 
          				errors, and system errors.	
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="SOACommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>

				<xsd:restriction base="xsd:token">
					<xsd:enumeration value="System">
					  <xsd:annotation>
					    <xsd:documentation>
					      System error.
					    </xsd:documentation>
                      </xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="Application">
					  <xsd:annotation>
					    <xsd:documentation>
					      Application error.
					    </xsd:documentation>
                      </xsd:annotation>					
					</xsd:enumeration>
					<xsd:enumeration value="Request">
					  <xsd:annotation>
					    <xsd:documentation>
					      Request error.
					    </xsd:documentation>
                      </xsd:annotation>					
					</xsd:enumeration>
				</xsd:restriction>

			</xsd:simpleType>
			<xsd:complexType name="ErrorData">
				<xsd:annotation>
					<xsd:documentation>A container for error details.
					</xsd:documentation>

					<xsd:appinfo>
						<typeLibrarySource library="SOACommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>

				<xsd:sequence>
					<xsd:element name="errorId" type="xsd:long">
					<xsd:annotation>
					<xsd:documentation>
          A unique code that identifies the particular error condition that
          occurred. Your application can use error codes as identifiers in your
          customized error-handling algorithms.
          			</xsd:documentation>
          			</xsd:annotation>
					</xsd:element>
					
					<xsd:element name="domain" type="xsd:string">
					<xsd:annotation>
					<xsd:documentation>
          Name of the domain in which the error occurred.
          &lt;dl&gt;
          &lt;dt&gt;
            &lt;b&gt;domain values:&lt;/b&gt;
          &lt;/dt&gt;
          &lt;dt&gt;
            Marketplace
          &lt;/dt&gt;
          &lt;dd&gt;
            A business or validation error occurred in the service.
          &lt;/dd&gt;
          &lt;dt&gt;
            SOA 
          &lt;/dt&gt;
          &lt;dd&gt;
            An exception occurred in the SOA (Service Oriented Architecture)
            framework.
          &lt;/dd&gt;
          &lt;/dl&gt;	
          			</xsd:documentation>
          			</xsd:annotation>
					</xsd:element>
					
					<xsd:element minOccurs="0" name="subdomain" type="xsd:string">
					<xsd:annotation>
					<xsd:documentation>
          Name of the subdomain in which the error occurred:
          &lt;dl&gt;
          &lt;dt&gt;Subdomain values:&lt;/dt&gt;
          &lt;dt&gt;&lt;i&gt;ThisServiceDomain&lt;/i&gt;&lt;/dt&gt;
          &lt;dd&gt;The error is specific to this service.&lt;/dd&gt;
          &lt;dt&gt;MarketplaceCommon&lt;/dt&gt;
          &lt;dd&gt;The error is common to all services.&lt;/dd&gt;
          &lt;/dl&gt;	
          			</xsd:documentation>
          			</xsd:annotation>
					</xsd:element>
					
					<xsd:element name="severity" type="tns:ErrorSeverity">
					<xsd:annotation>
					<xsd:documentation>
          Indicates whether the reported problem is fatal (an error) or is less-
          severe (a warning). Review the error message details for information on
          the cause.
          &lt;br&gt;&lt;br&gt;
          If the request fails and the application is the source of the error (for
          example, a required element is missing), update the application before
          you retry the request. If the problem is due to incorrect user data,
          alert the end-user to the problem and provide the means for them to
          correct the data. 
          &lt;br&gt;&lt;br&gt;
          If the source of the problem is in the service, you can retry the request
          a reasonable number of times (try the request twice).
          After the problem is resolved, you can resend the request in its original
          form.
          &lt;br&gt;&lt;br&gt;
          If a warning occurs, warning information is returned in addition to the
          business data. Normally, you do not need to resend the request (as the
          original request was successful).	
          			</xsd:documentation>
          			</xsd:annotation> 					
					</xsd:element>
					
					<xsd:element name="category" type="tns:ErrorCategory">
					<xsd:annotation>
					<xsd:documentation>
          There are three categories of errors: Request errors, Application 
          errors, and System errors.		
          			</xsd:documentation>
          			</xsd:annotation>				
					</xsd:element>
					
					<xsd:element name="message" type="xsd:string">
					<xsd:annotation>
					<xsd:documentation>
					A detailed description of the condition that caused the error.		
          			</xsd:documentation>
          			</xsd:annotation>
					</xsd:element>
					
					<xsd:element minOccurs="0" name="exceptionId" type="xsd:token">
					<xsd:annotation>
					<xsd:documentation>
					Unique identifier for an exception associated with an error.		
          			</xsd:documentation>
          			</xsd:annotation>
					</xsd:element>
					
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="parameter" type="tns:ErrorParameter">
					<xsd:annotation>
					<xsd:documentation>
          Various warning and error messages that return one or more variables that 
          contain contextual information about the error. This is often the 
          field or value that triggered the error.				
          			</xsd:documentation>
          			</xsd:annotation>				
					</xsd:element>
				</xsd:sequence>


			</xsd:complexType>

			<xsd:complexType name="ErrorMessage">
				<xsd:annotation>
					<xsd:documentation>
					Information regarding an error or warning that occurred when eBay processed
        the request. Not returned when the ack value is Success. Run-time errors are
        not reported here, but are instead reported as part of a SOAP fault.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="SOACommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>

				<!--

					processContents="lax" /> </xs:sequence>
				-->

				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="error" type="tns:ErrorData">
						<xsd:annotation>
         <xsd:documentation>
          Unbounded error.
         </xsd:documentation>
        </xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="ErrorParameter">
				<xsd:annotation>
					<xsd:documentation>Contextual data associated with an error.</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="SOACommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>

				<xsd:simpleContent>
					<xsd:extension base="xsd:string">
						<xsd:attribute name="name" type="xsd:string" use="optional">	
							<xsd:annotation>
							<xsd:documentation>
							Error name.
          					</xsd:documentation>
          					</xsd:annotation>
						</xsd:attribute>
					</xsd:extension>
				</xsd:simpleContent>
			</xsd:complexType>
			
			<xsd:simpleType name="ErrorSeverity">
				<xsd:annotation>
					<xsd:documentation>
        Indicates whether the error is a fatal error (causing the request to fail)
        or a less severe error (a warning) that should be communicated to the user.
			</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="SOACommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>

				<xsd:restriction base="xsd:token">
					<xsd:enumeration value="Error">
        <xsd:annotation>
          <xsd:documentation>
           Error type.
          </xsd:documentation>
        </xsd:annotation>					
					</xsd:enumeration>
					<xsd:enumeration value="Warning">
        <xsd:annotation>
          <xsd:documentation>
           Warning type.
          </xsd:documentation>
        </xsd:annotation>						
					</xsd:enumeration>
				</xsd:restriction>

			</xsd:simpleType>
			<xsd:complexType name="LookupResponse">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceResponse">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="0" name="longURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>The LongURI corresponding to a
										ShortURI.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="expirationDate" type="xsd:dateTime">
								<xsd:annotation>
									<xsd:documentation>Date until which this shortURI will be
										active.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="BaseURIEntry">
				<xsd:annotation>
					<xsd:documentation>Represents a Base URI Entry.
					</xsd:documentation>

					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="0" name="longURI" type="xsd:anyURI">
						<xsd:annotation>
							<xsd:documentation>LongURI is the actual URI that is
								shortened.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:element maxOccurs="1" minOccurs="0" name="shortURI" type="xsd:anyURI">
							<xsd:annotation>
								<xsd:documentation>The ShortURI is a combination of the baseURI
									and a handle.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element maxOccurs="1" minOccurs="0" name="handle" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Handle is a unique value for the 
									shortURI.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
					<xsd:element maxOccurs="1" minOccurs="0" name="namespace" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Namespace is used for Categorize the
								shortURI.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="tags" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Tags are used for identification of
								shortURI (max number of tags is 2).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:element maxOccurs="1" minOccurs="0" name="expirationDate" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>Date until which the ShortURI will be
									active.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element maxOccurs="1" minOccurs="0" name="durationInDays" type="xsd:unsignedInt">
							<xsd:annotation>
								<xsd:documentation>Number of days for the ShortURI to be
									active.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
					<xsd:element maxOccurs="1" minOccurs="0" name="isActive" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Indicates whether a shortURI is
								active or not.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="description" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Description for 
								 identifying and searching for a shortURI.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="CreateShortURIRequest">
				<xsd:annotation>
				<xsd:documentation>
				Creates a new ShortURI for a LongURI. The handle
				is an autogenerated base62 value.
				</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceRequest">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="1" name="longURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>The LongURI for which a shortURI is created.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="baseURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>
										The baseURI of the shortURI.
										Example:
										http://ebay.us/
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="namespace" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Namespace to categorize the
										shortURI.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="unbounded" minOccurs="0" name="tags" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Tags for identifying or
										searching for a shortURI.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:choice>
								<xsd:element maxOccurs="1" minOccurs="0" name="expirationDate" type="xsd:dateTime">
									<xsd:annotation>
										<xsd:documentation>Date until the shortURI expires.
											</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element maxOccurs="1" minOccurs="0" name="durationInDays" type="xsd:unsignedInt">
									<xsd:annotation>
										<xsd:documentation>Number of days that the shortURI
											is active.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:choice>
							<xsd:element maxOccurs="1" minOccurs="0" name="description" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Description for
										 identifying and searching for a shortURI.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>
			<xsd:complexType name="RegisterShortURIRequest">
				<xsd:annotation>
				<xsd:documentation>
				Creates a new ShortURI for a LongURI using the user's vanity handle. 
				The maximum number of tags allowed per shortURI is 2.
				</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceRequest">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="1" name="longURI" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>The LongURI for which a shortURI is created.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="baseURI" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>The baseURI of the shortURI.
										Example:
										http://ebay.us/
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="1" name="handle" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>User defined vanity handle. Maximum length
										is 10 characters.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="namespace" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Namespace to categorize the
										shortURI.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="unbounded" minOccurs="0" name="tags" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Tags  to identify or
										search for a shortURI.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:choice>
								<xsd:element maxOccurs="1" minOccurs="0" name="expirationDate" type="xsd:dateTime">
									<xsd:annotation>
										<xsd:documentation>Date until this shortURI expires.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element maxOccurs="1" minOccurs="0" name="durationInDays" type="xsd:unsignedInt">
									<xsd:annotation>
										<xsd:documentation>Number of days that the shortURI
											is active.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:choice>
							<xsd:element maxOccurs="1" minOccurs="0" name="description" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Description for
										identifying and searching for a shortURI.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>
			<xsd:complexType name="SearchRequest">
				<xsd:annotation>
				<xsd:documentation>
				Searches for a baseURI, namespace, and Tags. This is an AND based 
				search of all three criteria.
				</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceRequest">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="0" name="baseURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>The baseURI of the shortURI. Example:
										http://ebay.us/</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="namespace" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>The namespace of the shortURI.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="unbounded" minOccurs="0" name="tags" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Tags related to the shortURI.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="description" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Description for
										identifying or searching for a shortURI.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>
			<xsd:complexType abstract="true" name="BaseServiceRequest">
				<xsd:annotation>
					<xsd:documentation>
						The base class for a request container for all service
						operations.
			</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="MarketPlaceServiceCommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:sequence>
					
				</xsd:sequence>
			</xsd:complexType>
			<xsd:simpleType name="TagModeValue">
				<xsd:annotation>
					<xsd:documentation>Represents the tagUpdate Mode, whether
						it is ADD or REPLACE</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="ADD">
						<xsd:annotation>
							<xsd:documentation>Option for adding or appending tags to
								the existing ones.</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="REPLACE">
						<xsd:annotation>
							<xsd:documentation>Option for replacing existing tags with
								new tags.</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:complexType name="UpdateRequest">
				<xsd:annotation>
				<xsd:documentation>
				Updates the namespace and tag information for a  
				shortURI. The maximum number of tags per shortURI is 2.
				</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceRequest">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="1" name="shortURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>ShortURI that needs to be updated.
										Example: http://ebay.us/xyz123</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="namespace" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Namespace to update for a ShortURI.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="unbounded" minOccurs="0" name="tags" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Tags to add or replace for a shortURI.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="1" name="tagUpdateMode" type="tns:TagModeValue">
								<xsd:annotation>
									<xsd:documentation>TagMode that describes whether the request is
										an ADD or REPLACE of tags.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="description" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Description for identifying or searching for
										a shortURI.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>
		</xsd:schema>
