			<xsd:complexType name="GetStatsRequest">
				<xsd:annotation>
				<xsd:documentation>
				Returns statistics for a shortURI, which includes the total number of
				clicks, and clicks grouped by browser, referrer, and time.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceRequest">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="1" name="shortURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>
										The ShortURI for which
										Statistics is needed.
										example:
										http://ebay.us/xyz123
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="GetStatsResponse">
				<xsd:annotation>
				<xsd:documentation>
					Gets the statistics information about a ShortURI request.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceResponse">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="0" name="uriStats" type="tns:URIStats">
								<xsd:annotation>
									<xsd:documentation>
										Statistics for a
										ShortURI.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="URIStats">
				<xsd:annotation>
					<xsd:documentation>
						Provides URI statistics.
					</xsd:documentation>

					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="0" name="shortURI" type="xsd:anyURI">
						<xsd:annotation>
							<xsd:documentation>
								ShortURI for the statistics.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="totalClicks" type="xsd:unsignedLong">
						<xsd:annotation>
							<xsd:documentation>
								Total number of times that the ShortURI is
								accessed.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="statsDetail" type="tns:URIStatsDetail">
						<xsd:annotation>
							<xsd:documentation>
								Statistics Grouped by User-Agent,
								Referrer, and
								Location.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="URIStatsDetail">
				<xsd:annotation>
					<xsd:documentation>
						Represents URI statistics detail.
					</xsd:documentation>

					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" name="clicks" type="xsd:unsignedLong">
						<xsd:annotation>
						<xsd:documentation>
					    Number of clicks on the short URI.
					    </xsd:documentation>
					    </xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:element maxOccurs="1" minOccurs="1" name="referrer" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>
									Web site that referred this shortURI.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element maxOccurs="1" minOccurs="1" name="useragent" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>
									Browser used by the client.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element maxOccurs="1" minOccurs="1" name="location" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>
									Geographical location of the client.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element maxOccurs="1" minOccurs="1" name="time" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>
									Time when the ShortURI was accessed.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
				</xsd:sequence>
			</xsd:complexType>

