<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="ShortURIService" targetNamespace="http://www.ebay.com/marketplace/services/shorturiservice" xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://www.ebay.com/marketplace/services/shorturiservice" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns="http://schemas.xmlsoap.org/wsdl/">
  <wsdl:types>
    <xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.ebay.com/marketplace/services/shorturiservice">

			<xsd:simpleType name="AckValue">
				<xsd:annotation>
					<xsd:documentation>
						Indicates whether the error is a severe error
						(causing the request to fail) or an
						informational error (a warning) that should be
						communicated to the user.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="MarketPlaceServiceCommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Success">
						<xsd:annotation>
							<xsd:documentation>
								The request was processed successfully.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="Failure">
						<xsd:annotation>
							<xsd:documentation>
								The request that triggered the error was
								not processed successfully. When a
								serious application-level error occurs,
								the error is returned instead of the
								business data.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="Warning">
						<xsd:annotation>
							<xsd:documentation>
								The request that triggered the error was
								processed successfully but with some
								warnings.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="PartialFailure">
						<xsd:annotation>
							<xsd:documentation>
								The request that triggered the error was
								processed successfully but with some partial Errors.(For Future use)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:complexType abstract="true" name="BaseServiceRequest">
				<xsd:annotation>
					<xsd:documentation>
						This is the base class for request container for
						all service operations.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="MarketPlaceServiceCommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:any maxOccurs="unbounded" minOccurs="0" processContents="lax"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType abstract="true" name="BaseServiceResponse">
				<xsd:annotation>
					<xsd:documentation>
						Base response container for all service
						operations. Contains error information
						associated with the request.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="MarketPlaceServiceCommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="ack" type="tns:AckValue">
						<xsd:annotation>
							<xsd:documentation>
								Acknowledgement returned in the Response, which indicates whether the operation was Successful or not.
							</xsd:documentation>
							<xsd:appinfo>
								<CallInfo>
									<AllCalls/>
									<Returned>Always</Returned>
								</CallInfo>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="errorMessage" type="tns:ErrorMessage">
						<xsd:annotation>
							<xsd:documentation>
								Information for an error or warning that
								occurred when eBay processed the
								request.
							</xsd:documentation>
							<xsd:appinfo>
								<CallInfo>
									<AllCalls/>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="version" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>
								version of the data. (for future use)
							</xsd:documentation>
							<xsd:appinfo>
								<CallInfo>
									<AllCalls/>
									<Returned>Always</Returned>
								</CallInfo>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" name="timestamp" type="xsd:dateTime">
						<xsd:annotation>
							<xsd:documentation>
								Timestamp of the operation.
							</xsd:documentation>
							<xsd:appinfo>
								<CallInfo>
									<AllCalls/>
									<Returned>Always</Returned>
								</CallInfo>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:any maxOccurs="unbounded" minOccurs="0" processContents="lax"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:simpleType name="ErrorCategory">
				<xsd:annotation>
					<xsd:documentation>
						Indicates the Category of the Error.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="SOACommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:restriction base="xsd:token">
					<xsd:enumeration value="System"/>
					<xsd:enumeration value="Application"/>
					<xsd:enumeration value="Request"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:complexType name="ErrorData">
				<xsd:annotation>
					<xsd:documentation>
						The ErrorData which Contains information of the Error like ErrorID, Domain,Error Message etc.,.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="SOACommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="errorId" type="xsd:long"/>
					<xsd:element name="domain" type="xsd:string"/>
					<xsd:element minOccurs="0" name="subdomain" type="xsd:string"/>
					<xsd:element name="severity" type="tns:ErrorSeverity"/>
					<xsd:element name="category" type="tns:ErrorCategory"/>
					<xsd:element name="message" type="xsd:string"/>
					<xsd:element minOccurs="0" name="exceptionId" type="xsd:token"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="parameter" type="tns:ErrorParameter"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="ErrorMessage">
				<xsd:annotation>
					<xsd:documentation>
						ErrorMessage contains a list of Error Data.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="SOACommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>
				<!-- 
					<xsd:sequence>
					<xsd:any maxOccurs="unbounded" minOccurs="0"
					processContents="lax" />
					</xsd:sequence>
				-->
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="error" type="tns:ErrorData"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="ErrorParameter">
				<xsd:annotation>
					<xsd:documentation>
						ErrorParameters contains the Parameter which caused the error.
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="SOACommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:simpleContent>
					<xsd:extension base="xsd:string">
						<xsd:attribute name="name" type="xsd:string" use="optional"/>
					</xsd:extension>
				</xsd:simpleContent>
			</xsd:complexType>
			<xsd:simpleType name="ErrorSeverity">
				<xsd:annotation>
					<xsd:documentation>
						Indicates the Severity of the Error
					</xsd:documentation>
					<xsd:appinfo>
						<typeLibrarySource library="SOACommonTypeLibrary" namespace="http://www.ebay.com/marketplace/services"/>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:restriction base="xsd:token">
					<xsd:enumeration value="Error"/>
					<xsd:enumeration value="Warning"/>
				</xsd:restriction>
			</xsd:simpleType>

			<xsd:complexType name="BaseURIEntry">
				<xsd:annotation>
					<xsd:documentation>
						Represents a Base URI Entry
					</xsd:documentation>

					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="0" name="longURI" type="xsd:anyURI">
						<xsd:annotation>
							<xsd:documentation>
								LongURI is the actual URI which is
								shortened
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:element maxOccurs="1" minOccurs="0" name="shortURI" type="xsd:anyURI">
							<xsd:annotation>
								<xsd:documentation>
									ShortURI is a combination of baseuri
									and handle
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element maxOccurs="1" minOccurs="0" name="handle" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>
									Handle is a unique value for the
									each shortURI
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
					<xsd:element maxOccurs="1" minOccurs="0" name="namespace" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>
								Namespace is used for Categorize the
								shortURI
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="tags" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>
								Tags are used for identification of
								shortURI(Max No. of Tags : 2)
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:element maxOccurs="1" minOccurs="0" name="expirationDate" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>
									Date till which the shorturi will be
									active
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element maxOccurs="1" minOccurs="0" name="durationInDays" type="xsd:unsignedInt">
							<xsd:annotation>
								<xsd:documentation>
									No of days for which the ShortURI
									should be active
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
					<xsd:element maxOccurs="1" minOccurs="0" name="isActive" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>
								Indicates whether a shortURI is active
								or not
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="CreateShortURIRequest">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceRequest">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="1" name="longURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>
										The LongURI for which shortURI
										is created
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="namespace" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>
										Namespace to categorize the
										shortURI
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="unbounded" minOccurs="0" name="tags" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>
										tags used for ease of
										identifying or searching for an
										shortURI
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:choice>
								<xsd:element maxOccurs="1" minOccurs="0" name="expirationDate" type="xsd:dateTime">
									<xsd:annotation>
										<xsd:documentation>
											Date till which this
											shortURI will be active
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element maxOccurs="1" minOccurs="0" name="durationInDays" type="xsd:unsignedInt">
									<xsd:annotation>
										<xsd:documentation>
											Number of days for which the
											shortURI will be active
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:choice>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="CreateShortURIResponse">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceResponse">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="0" name="shortURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>
										The Generated ShortURI for the
										given URI
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="DeleteRequest">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceRequest">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="1" name="shortURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>
										ShortURI which needs to be
										deactivated.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="DeleteResponse">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceResponse">
						<xsd:sequence>

						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="ExtendedURIEntry">
				<xsd:annotation>
					<xsd:documentation>
						Represents an ExtendedURI Entry
					</xsd:documentation>

					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseURIEntry">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="0" name="creator" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>
										The APPID which creates the
										ShortURI
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="creationDate" type="xsd:dateTime">
								<xsd:annotation>
									<xsd:documentation>
										Date of Creation of ShortURI
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element maxOccurs="1" minOccurs="0" name="modificationDate" type="xsd:dateTime">
								<xsd:annotation>
									<xsd:documentation>
										Last Date of Modification of
										ShortURI
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="GetInfoLongRequest">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceRequest">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="1" name="longURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>
										The LongURI for which
										information is needed.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="GetInfoLongResponse">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceResponse">
						<xsd:sequence>
							<xsd:element maxOccurs="unbounded" minOccurs="0" name="uriEntry" type="tns:ExtendedURIEntry">
								<xsd:annotation>
									<xsd:documentation>
										Information about the given
										LongURI
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="GetInfoShortRequest">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceRequest">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="1" name="shortURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>
										The ShortURI for which
										information is needed. example:
										http://ebay.us/xyz123
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="GetInfoShortResponse">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceResponse">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="0" name="uriEntry" type="tns:ExtendedURIEntry">
								<xsd:annotation>
									<xsd:documentation>
										Information about the given
										ShortURI
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="GetStatsRequest">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceRequest">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="1" name="shortURI" type="xsd:anyURI">
								<xsd:annotation>
									<xsd:documentation>
										The ShortURI for which
										Statistics is needed. example:
										http://ebay.us/xyz123
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="GetStatsResponse">
				<xsd:annotation>
					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="tns:BaseServiceResponse">
						<xsd:sequence>
							<xsd:element maxOccurs="1" minOccurs="0" name="uriStats" type="tns:URIStats">
								<xsd:annotation>
									<xsd:documentation>
										Statistics for the which given
										ShortURI
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="URIStats">
				<xsd:annotation>
					<xsd:documentation>
						Represents URI Statistics
					</xsd:documentation>

					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="0" name="shortURI" type="xsd:anyURI">
						<xsd:annotation>
							<xsd:documentation>
								ShortURI for which statistics is given
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="totalClicks" type="xsd:unsignedLong">
						<xsd:annotation>
							<xsd:documentation>
								Total number of times, the shortURI is
								accessed
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="statsDetail" type="tns:URIStatsDetail">
						<xsd:annotation>
							<xsd:documentation>
								Statistics Grouped by User-Agent,
								Referrer and Location
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="URIStatsDetail">
				<xsd:annotation>
					<xsd:documentation>
						Represents URI Statistics Detail
					</xsd:documentation>

					<xsd:appinfo>
						<typeLibrarySource library="ShortURITypeLibrary" namespace="http://www.ebay.com/marketplace/services/shorturi">
						</typeLibrarySource>
					</xsd:appinfo>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" name="clicks" type="xsd:unsignedLong">
					</xsd:element>
					<xsd:choice>
						<xsd:element maxOccurs="1" minOccurs="1" name="referrer" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>
									Website which referred this shortURI
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element maxOccurs="1" minOccurs="1" name="useragent" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>
									Browser used by the client
