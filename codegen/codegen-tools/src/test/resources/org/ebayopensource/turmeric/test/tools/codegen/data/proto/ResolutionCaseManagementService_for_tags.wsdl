				</xs:documentation>
				<xs:appinfo>
					<tagStatus>ComingSoon</tagStatus>
					<callInfo>
						<callName>
							getEBPCaseDetail
						</callName>
						<returned>Conditionally</returned>
					</callInfo>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
		<xs:element name="decisionDate" type="xs:dateTime" minOccurs="0">
